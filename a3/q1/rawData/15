function cansubmit(theform) {
   if ( document.getElementById('myagree').checked == false ) {
      document.getElementById('mustagree').style.display='block';
      return false;
   }
   if ( document.getElementById('confirmvegan').checked == false ) {
      document.getElementById('mustconfirmvegan').style.display='block';
      return false;
   }
   if ( document.getElementById('fvupload').value == "" ) {
      alert('You must select a file!');
      return false;
   }
   if ( document.getElementById('caption').value == "" ) {
      alert('You must enter a caption!');
      return false;
   }
