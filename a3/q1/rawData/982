<body class=' section-news'>

                        
                                
<script type="text/javascript">
    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];

    (function() {
        var gads = document.createElement('script');
        gads.async = true;
        gads.type = 'text/javascript';
        var useSSL = 'https:' == document.location.protocol;
        gads.src = (useSSL ? 'https:' : 'http:') +'//www.googletagservices.com/tag/js/gpt.js';
        var node = document.getElementsByTagName('script')[0];
        node.parentNode.insertBefore(gads, node);
    })();

    quantSegs=[];
    function qc_results(result) {
        for (var i = 0; i < result.segments.length; i++) {
            quantSegs.push(result.segments[i].id);
        }
    }
</script>

<script type="text/javascript" src="http://segapi.quantserve.com/api/segments.json?a=p-880wF3y-pNyF6&callback=qc_results"></script>

<script type="text/javascript">
    googletag.cmd.push(function() {
                    googletag.pubads().setTargeting("category", "news");
                    googletag.pubads().setTargeting("subcategory", "so-that-happened");
                    googletag.pubads().setTargeting("page-type", "article");
                    googletag.pubads().setTargeting("content-type", "editorial");
                    googletag.pubads().setTargeting("content-id", "article-3722886");
                    googletag.pubads().setTargeting("is-ad-friendly", "yes");
                    googletag.pubads().setTargeting("env", "production");
        
        googletag.pubads().setTargeting("qcsegs",quantSegs);

        if (typeof someecards_config.google_tags != "undefined") {
            for (var key in someecards_config.google_tags) {
                if (someecards_config.google_tags.hasOwnProperty(key)) {
                    googletag.pubads().setTargeting(key, someecards_config.google_tags[key]);
                }
            }
        }

        googletag.pubads().enableSingleRequest();
        googletag.pubads().disableInitialLoad();
        googletag.enableServices();
    });

    var sec_global_targeting = {};
            sec_global_targeting['category'] = 'news';
            sec_global_targeting['subcategory'] = 'so-that-happened';
