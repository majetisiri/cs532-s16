<script type="text/javascript">
    window.fbAsyncInit = function() {
        FB.init({
            appId       : 32911495680,
            version    : 'v2.5',
            status      : true,
            cookie      : true,
            xfbml       : true,
            oauth       : true
        });

        var fbLoginChecked = true; // Temporarily turning this off while facebook has issues.
        FB.getLoginStatus(function(response) {
            if (response.status == 'connected' && fbLoginChecked != true) {
                if (userAuth.getUserData() == null) {
                    if (userAuth.getHasUpdatedUserData() == true) {
                        userAuth.autoLoginFB();
                    } else {
                        userAuth.updateUserData(function() {
                            if (userAuth.getUserData() == null) {
                                userAuth.autoLoginFB();
                            }
                        });
                    }
                }
            }

            fbLoginChecked = true;
        });

        FB.Event.subscribe('edge.create', function(liked_link, button) {
            if (Cookies.get('socialLike') == 'false') {
                ga('send', 'event', 'facebook-like-button', 'relike', $(button).data('ref'));
            } else {
                ga('send', 'event', 'facebook-like-button', 'like', $(button).data('ref'));
            }
            Cookies.set('socialLike', 'true', { expires: 365*10, domain: someecards_config.cookie_domain });
        });
        FB.Event.subscribe('edge.remove', function(liked_link, button) {
            ga('send', 'event', 'facebook-like-button', 'dislike', $(button).data('ref'));
            if (Cookies.get('socialLike')) {
                Cookies.remove('socialLike', { expires: 365*10, domain: someecards_config.cookie_domain });
            }

            // set counter so it will make sure the next page load gives them the modal
            Cookies.set('socialLikePageLoadCounter', someecards_config.page_loads_for_like_modal - 1, { expires: 365*10, domain: someecards_config.cookie_domain });
        });
        FB.Event.subscribe('xfbml.render', function() {
            someecards.showLikingPopup();
        });
    };
    (function(d, s, id){
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) {return;}
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/en_US/sdk.js";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
</script>

