<script type="text/javascript">
function CommaSeperatedCount(x) {
    return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
}
  
 $(document).ready(function () {


    var loadurl = "http://www.businessinsider.com/r-stay-on-sidelines-until-chipotle-sales-recover-analysts-2016-2" ;


  $.getJSON('http://cdn.api.twitter.com/1/urls/count.json?url=' + loadurl  + '&callback=?', function (twitdata) {
    }).done(function(twitdata){
        if(typeof(twitdata.count) != "undefined")
        {
          twitcount = CommaSeperatedCount(twitdata.count);
          //$('#total-shares').attr('data-facebookShares', fbdata.shares)
 
          $(".twittercc").html(twitcount);
           // this.find(".twittercc").html(twitcount);
         }else{
//                     this.find(".twittercc").html("0");
          $(".twittercc").html("0");
         }

    });



         $.getJSON( 'http://www.linkedin.com/countserv/count/share?url=' + loadurl + '&callback=?', function( linkdindata ) {
      }).done(function(linkdindata){
 if(typeof(linkdindata.count) != "undefined")
        {
          linkedincount = CommaSeperatedCount(linkdindata.count);
          //$('#total-shares').attr('data-facebookShares', fbdata.shares)
 
           $(".linkedincc").html(linkedincount);
         }else{
           $(".linkedincc").html("0");

         }
    });


  $.getJSON('http://graph.facebook.com/?id=' + loadurl, function (fbdata) {
    }).done(function(fbdata){
         if(typeof(fbdata.shares) != "undefined")
        {
          fbcount = CommaSeperatedCount(fbdata.shares);
          //$('#total-shares').attr('data-facebookShares', fbdata.shares)
           $(".facebookcc").html(fbcount);
         }else{

  $(".facebookcc").html("0");
         }

    });

 
});

</script>




