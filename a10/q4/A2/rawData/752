            var mobile_url = 'findusm';
            if ( screen.width <= 699 ) {
                var query_string = {};
                var query = window.location.href.toString();
                var type = query.split('?')[1];
                if ( type ) {
                   type = type.toString().split('=')[1];
                   mobile_url += '#/?type=' + type;
                }
                window.location.replace( mobile_url );
