    <script>
        $(document).ready(function() {
        //Sort random function
        function random(owlSelector){
        owlSelector.children().sort(function(){
        return Math.round(Math.random()) - 0.5;
        }).each(function(){
        $(this).appendTo(owlSelector);
        });
        }
         /* customizing see http://owlgraphic.com/owlcarousel/index.html */
        $("#owl-demo").owlCarousel({
        
		    items : 2, // 4 items above 1000px browser width
			itemsDesktop : [1600,2], // 2 items between 1400px and 1201px
			itemsDesktopSmall : [1200,2], // 2 items between 1200px and 801px
			itemsTablet: [800,1], //1 items between 800 and 0
			itemsMobile : false, // itemsMobile disabled - inherit from itemsTablet option


         autoPlay : true,
          lazyLoad : true,
          stopOnHover : true,
		  navigation : true,
		  pagination : false,
   			 beforeInit : function(elem){
        		//Parameter elem pointing to $("#owl-demo")
        			// random(elem);
        }
         
        });
         
        });
    </script>
    
        
