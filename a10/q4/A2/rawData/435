            src    : "http://g-ecx.images-amazon.com/images/G/01/img15/x-site/gb/22599_warehousedeals_double-promo._CB316932036_.jpg",
            height : 200,
            width  : 440
        }).css({
            display : 'inline',
            border  : 0
        });

        var $link = $('<a>').attr('href', '/b?node=10158976011').append($img);
        $('#ns_1X06X77DK2A42XSHGFH1_761_').append($link);

        if(isAUI) {
            $('#ns_1X06X77DK2A42XSHGFH1_761_').show();
        }
    };

    var loadAfter = 'af';

    if (window.P && window.P.AUI_BUILD_DATE) {
        P.when('A', loadAfter).execute(function(A) {
            fn(A.$, true);
        });
    } else if (window.amznJQ) {
        amznJQ.onReady('jQuery', function() {
            amznJQ.onReady(loadAfter, function() {
                fn(jQuery);
            });
        });
    }
}());
</script>

