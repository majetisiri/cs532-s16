~>
    <span class="a-size-medium a-color-base inlineBlock unitLineHeight">
        {{data.marketingMessage}}
    </span>
<~
    } else if(data.deal.pricingData.prices.dealPrice) {
        var price = data.deal.pricingData.prices.dealPrice.min.formattedValue;
        if(data.deal.pricingData.prices.dealPrice.min.value !== data.deal.pricingData.prices.dealPrice.max.value) {
            price += ' - ' + data.deal.pricingData.prices.dealPrice.max.formattedValue;

            if(data.deal.detail.itemType === gb.enums.ItemTypes.VARIATION && data.asin) {
                price = data.asin.dealPriceFormatted;
            }
        }
        if(gb.utils.showPPU(data.deal.dealID) && data.deal.pricingData.unitPrice && data.deal.pricingData.baseUnit) {
                price += ' ' + gb.resources.getString('gb_ppu_price', {
                        price     : data.deal.pricingData.unitPrice.formattedValue,
                        unitValue : data.deal.pricingData.baseUnit.value,
                        unitName  : data.deal.pricingData.baseUnit.name
                    });
        }
~>
        <span class="a-color-{{textColor}} heading4 inlineBlock unitLineHeight">
            {{price}}
        </span>
<~
    }
~>
