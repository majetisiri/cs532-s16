    if(data.deal.dealType === gb.enums.DealTypes.DOTD && (data.deal.dealState === gb.enums.DealStates.SOLDOUT || data.deal.dealState === gb.enums.DealStates.EXPIRED))
    {
        textColor = 'secondary';
    }
