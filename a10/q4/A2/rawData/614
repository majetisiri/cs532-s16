var getJSONP = function ( url, callback ) {
	var localurl = "/scripts/getjsonp.js?v=1";
    var ref = window.document.getElementsByTagName( 'script' )[ 0 ];
    var script = window.document.createElement( 'script' );
    script.src = localurl;
	script.async = true;
	script.setAttribute("data-url", url);
	script.setAttribute("data-callback", callback);
    ref.parentNode.insertBefore( script, ref );
};

var countFacebook = function ( resultArray ) {
	if(resultArray['shares']==0){ return;} else if(typeof resultArray['shares']=='undefined'){ return;}else if(typeof window.socialCountsArray['facebook'] == 'undefined'){ window.socialCountsArray['facebook'] = resultArray['shares']; } else if ( window.socialCountsArray['facebook'] > 0 && window.socialCountsArray['facebook']!=resultArray['shares']) { window.socialCountsArray['facebook'] = window.socialCountsArray['facebook'] + resultArray['shares'];} var countElements = document.getElementsByClassName("social_facebook_count"); for(var i=0; i<countElements.length; i++){ countElements[i].innerHTML = window.socialCountsArray['facebook'];}
}

var countTwitter = function ( resultArray ) {
	if(resultArray['count']==0){ return; } else if(typeof window.socialCountsArray['twitter'] == 'undefined'){ window.socialCountsArray['twitter'] = resultArray['count']; } else if ( window.socialCountsArray['twitter'] > 0 && window.socialCountsArray['twitter']!=resultArray['count']) { window.socialCountsArray['twitter'] = window.socialCountsArray['twitter'] + resultArray['count'];} var countElements = document.getElementsByClassName("social_twitter_count"); for(var i=0; i<countElements.length; i++){ countElements[i].innerHTML = window.socialCountsArray['twitter'];}
}

var countGooglePlus = function ( count ) {
	if(count==0){ return;} else if(typeof window.socialCountsArray['googleplus'] == 'undefined'){ window.socialCountsArray['googleplus'] = count; } else if ( window.socialCountsArray['googleplus'] > 0 && window.socialCountsArray['googleplus']!=count) { window.socialCountsArray['googleplus'] = window.socialCountsArray['googleplus'] + count;} var countElements = document.getElementsByClassName("social_googleplus_count"); for(var i=0; i<countElements.length; i++){ countElements[i].innerHTML = window.socialCountsArray['googleplus'];}
}

var countPinterest = function ( resultArray ) {
	if(resultArray['count']==0){ return; } else if(typeof window.socialCountsArray['pinterest'] == 'undefined'){ window.socialCountsArray['pinterest'] = resultArray['count']; } else if ( window.socialCountsArray['pinterest'] > 0 && window.socialCountsArray['pinterest']!=resultArray['count']) { window.socialCountsArray['pinterest'] = window.socialCountsArray['pinterest'] + resultArray['count'];} var countElements = document.getElementsByClassName("social_pinterest_count"); for(var i=0; i<countElements.length; i++){ countElements[i].innerHTML = window.socialCountsArray['pinterest'];}
}

var countLinkedIn = function ( resultArray ) {
	if(resultArray['count']==0){ return; } else if(typeof window.socialCountsArray['linkedin'] == 'undefined'){ window.socialCountsArray['linkedin'] = resultArray['count']; } else if ( window.socialCountsArray['linkedin'] > 0 && window.socialCountsArray['linkedin']!=resultArray['count']) { window.socialCountsArray['linkedin'] = window.socialCountsArray['linkedin'] + resultArray['count'];} var countElements = document.getElementsByClassName("social_linkedin_count"); for(var i=0; i<countElements.length; i++){ countElements[i].innerHTML = window.socialCountsArray['linkedin'];}
}

function moreSocial(){
	document.getElementsByClassName('social_tumblr').item(0).style.display="inline-block";
	document.getElementsByClassName('social_pinterest').item(0).style.display="inline-block";
	document.getElementsByClassName('social_linkedin').item(0).style.display="inline-block";
	document.getElementsByClassName('social_mail').item(0).style.display="inline-block";
	document.getElementsByClassName('social_more').item(0).style.display="none";
}

function reportspam(coid) {	
	if(coid!=''){
		var poststr = "coid=" + encodeURI( coid );	
		var spam_coid = 'spam_' + coid;
		document.getElementById(spam_coid).innerHTML="<span style='color: red'>Marked as Spam</span>";
    	makePOSTRequest('/scripts/reportspam.php', poststr, alertContents);
	}
}

function likecomment(coid) {	
	if(coid!=''){
		var poststr = "coid=" + encodeURI( coid );	
		var like_coid = 'like_' + coid;
		var comment_coid_likes = 'comment_' + coid + '_likes';
		var likes = document.getElementById(comment_coid_likes).innerHTML;
		if(likes==''){ likes = 0; }
		var likes = parseInt(likes) + 1;
		document.getElementById(like_coid).innerHTML="<span style='color: #0c0;'>Liked</span>";
		document.getElementById(like_coid).href="javascript:";
		document.getElementById(comment_coid_likes).innerHTML=likes;
    	makePOSTRequest('/scripts/likecomment.php', poststr, alertContents);
	}
}

function addcomment() {	
	if(document.getElementById("comment").value!=''){
		document.getElementById("form_comment").submit();
	}
}

function share(network){
	if(network=="facebook"){
		window.open("https://www.facebook.com/sharer/sharer.php?u=" + encodeURI("http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video"),"Facebook Share", "width=550,height=470,toolbar=no");
	}
	else if(network=="twitter"){
		window.open("https://twitter.com/intent/tweet?original_referer=" + encodeURI("http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video") + "&text=" + encodeURI("You Can Now Summon Your Tesla Using the Apple Watch [Video]") + "&tw_p=tweetbutton&url=http%3A%2F%2FiClarified.com%2F53775&via=iClarified","Tweet", "width=550,height=470,toolbar=no");
	}
	else if(network=="googleplus"){
		window.open("https://plus.google.com/share?url=" + encodeURI("http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video"),
  '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
	}
	else if(network=="tumblr"){
		window.open("http://www.tumblr.com/share/photo?source=" + encodeURI("http://www.iclarified.com/images/news/53775/53775/53775-1280.jpg") + "&caption=" + encodeURI('<h2><a target="_blank" href="http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video">You Can Now Summon Your Tesla Using the Apple Watch [Video]</a></h2><p>Rego Apps has released an update to its Remote S application for the Apple Watch which lets you summon your car, display battery usage, and trigger HomeLink.</p><p><a target="_blank" href="http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video">Read More</a><br></p>') + '&clickthru=http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video');
	}
	else if(network=="linkedin"){
		window.open("https://www.linkedin.com/shareArticle?mini=true&url=" + encodeURI("http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video") + "&title=" + encodeURI("You Can Now Summon Your Tesla Using the Apple Watch [Video]") + "&summary=" + encodeURI("Rego Apps has released an update to its Remote S application for the Apple Watch which lets you summon your car, display battery usage, and trigger HomeLink.") + "&source=iClarified","LinkedIn","width=550,height=470,toolbar=no");
	}
	else if(network=="pinterest"){
		window.open("http://www.pinterest.com/pin/create/button/?url=" + encodeURI("http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video") + "&media=" + encodeURI("http://www.iclarified.com/images/news/53775/53775/53775-1280.jpg") + "&description=" + encodeURI("You Can Now Summon Your Tesla Using the Apple Watch [Video]" + " - http://iClarified.com/53775 - Rego Apps has released an update to its Remote S application for the Apple Watch which lets you summon your car, display battery usage, and trigger HomeLink."),"Pinterest","toolbar=no,width=750,height=570");
	}
	else if(network=="mail"){
		window.open("/entry/email.php?enid=53775");
	}
}

</script>

<script>
var psid = "1455580515410619";
var push_check = "0";
document.body.onload = function() {
	if( push_check == "1" ) {
    	if ('safari' in window && 'pushNotification' in window.safari) {
        	var permissionData = window.safari.pushNotification.permission('web.com.iclarified.iclarified');
        	checkRemotePermission(permissionData);
    	}
	}
};
 
var checkRemotePermission = function (permissionData) {
    if (permissionData.permission === 'default') {
        window.safari.pushNotification.requestPermission('https://www.iclarified.com/scripts/push_safari', 			'web.com.iclarified.iclarified', {"psid": psid}, checkRemotePermission);
    }
    else if (permissionData.permission === 'denied') {
        // The user said no.
    }
    else if (permissionData.permission === 'granted') {
        // The web service URL is a valid push provider, and the user said yes.
    }
};
</script>
<script async type="text/javascript" src="../scripts/googleanalytics.js"></script>
</body></html>