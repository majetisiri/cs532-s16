   googletag.cmd.push( function() {
      yieldbot.pub( 'bc24' );
      var yieldbotAds = [];
      var numRailAdsToInclude = 2;
      for( var ad in gptadslots ) {
         if( gptadslots.hasOwnProperty( ad ) ) {
            switch( ad ) {
               case 'zone-none-block-1-leaderboard':
               case 'zone-none-block-3-leaderboard':
                  yieldbotAds.push( ad );
                  break;
               case 'zone-2-block-1-vertical':
               case 'zone-2-block-2-vertical':
               case 'zone-2-block-3-vertical':
               case 'zone-2-block-4-vertical':
                  if( numRailAdsToInclude > 0 ) {
                     yieldbotAds.push( ad );
                  }
                  numRailAdsToInclude--;
                  break;
               default:
                  break;
            }
         }
      }
      for( var i = 0; i < yieldbotAds.length; i++ ) {
         yieldbot.defineSlot( yieldbotAds[i] );
      }

      yieldbot.go();

      for( var i = 0; i < yieldbotAds.length; i++ ) {
         if( typeof gptadslots[yieldbotAds[i]] !== 'undefined' ) {
            yieldbot.setSlotTargeting( yieldbotAds[i], gptadslots[yieldbotAds[i]] );
         }
      }
