<script type="text/javascript">

function adInView( adID ) {

	var element = document.getElementById( adID );

    if( element == null ) {
        return;
    }

    if( isElementInViewport( element ) ) {
        googletag.cmd.push( function() {
            googletag.display( adID );
        });
    }
    else {
        element.className += ' notLoaded';
    }
}

$(function() {

    $('.notLoaded').appear();

    $(document.body).on('appear', '.notLoaded',function(event, $all_appeared_elements) {
        var me = $(this).attr('id');
        $(this).removeClass('notLoaded');
        googletag.cmd.push(function() {
            googletag.display(me);
        });
    });
});

function isElementInViewport (el) {

    //special bonus for those using jQuery
    if (typeof jQuery === "function" && el instanceof jQuery) {
        el = el[0];
    }

    var rect = el.getBoundingClientRect();

    return (
        rect.top >= 0 &&
        rect.left >= 0 &&
        rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
        rect.right <= (window.innerWidth || document.documentElement.clientWidth)
    );
}


  if(window.opt_disableAds === undefined || window.opt_disableAds == false) { // allow optimizely to disable ads
    googletag.cmd.push(function() {

            googletag.pubads().collapseEmptyDivs( true );
            googletag.pubads().enableAsyncRendering();
      googletag.enableServices();
    });
  }

  $(document).ready(function(){
    if( showSidewaysPage ) {
        activate( '.sideways-ad' );
    } else {
        activate( '.inline-ad' );
    }
  });

  function activate( selector ) {
    $(selector).each(function(){
      var me = $(this).attr('id');
      if(me != undefined && me != null && me != ''){
        adInView(me);
      }
    });
  }

</script>


    
