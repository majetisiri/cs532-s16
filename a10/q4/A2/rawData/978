<script type="text/javascript">
//
// Referrer/Sideways page: 
// If user comes from non-startribune domain, they get a different ad configuration when the article is over 24-hours old.
// The backend will always load the sideways ad data; the frontend determines whether to request these ads.
//

var showSidewaysPage = referrerCheck() ? true : false;

if( window.opt_sideways ) {
    showSidewaysPage = true;    // optimizely override
}

function referrerCheck() {
  var referrer = document.referrer;
  if( referrer === '' ) {
    return false;
  }
  if (referrer.indexOf('http://') === 0) {
    referrer = referrer.slice(7);
  }
  else if (referrer.indexOf('https://') === 0) {
    referrer = referrer.slice(8);
  }
  referrer = referrer.split('/')[0];

  var isStribDomain = false;
  stribDomains = [
    'startribune.com',
    'startribune.local',
    'startribune.dev',
    'strib.us',
    'vita.mn',
    'outbrain.com',
    'github.com',
    'slack.com',
    'squishlist.com'
  ];
  
  for (var i = 0; i < stribDomains.length; i++) {
    if (referrer.indexOf(stribDomains[i]) > -1) {
      isStribDomain = true;
    }
  }
  var isSearchBot = false;
  var userAgent = window.navigator.userAgent.toLowerCase();
  if (userAgent.indexOf( 'googlebot' ) > -1 || userAgent.indexOf( 'bingbot' ) > -1 ) {
    isSearchBot = true;
  }
  if (!isStribDomain && !isSearchBot) {  	
  	return true;
  }
 
  return false;
}

