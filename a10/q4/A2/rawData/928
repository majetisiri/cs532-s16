                if (numOfAds && numOfAds > 0 && numOfAds < 17){  mps._delays.refreshads = (300 * numOfAds); }
              }
            }
            mps._debug('[mps:Refresh] Refreshing [ALL] ad slots on page');
          } else {
            for(var i in slotname) {
              if(loadset==undefined && typeof(slotname[i])=='string') {
                //set delay to 0 if refreshing one ad
                if(slotname.length<1)  mps._delays.refreshads = 0;
                for(aok in mps._adobs) {
