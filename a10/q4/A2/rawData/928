                CNBC_Utils.execute('Surf Bedrock signin init', function(){
		CNBC_Surf.Signin_Ui.init();
		$('#login-container #signin').off().click(function(e){
			e.preventDefault();
			if(typeof SURF !== 'undefined') {
				CNBC_Surf_Mediator.init( { 
					'source' : 'default',
					'flow' : 'header'
				} );
				CNBC_Surf.Signin_Ui.startUIFlow('signin');
			}else {
				window.location.href = CNBC_Settings.baseurls.cnbc_base + CNBC_Surf.outageRedirection;
			}
		});
		$('#login-container .cnbcRegister a').off().click(function(e){
			e.preventDefault();
			if(typeof SURF !== 'undefined') {
				CNBC_Surf_Mediator.init( {
					'source' : 'default',
					'flow' : 'header'
				} );
				CNBC_Surf.Signin_Ui.startUIFlow('register', null, 1);
			}else {
				window.location.href = CNBC_Settings.baseurls.cnbc_base + CNBC_Surf.outageRedirection;
			}
			
		});

		CNBC_Surf_Mediator.init( {
			'source' : 'default',
			'flow' : 'header'
		} );
		
		if(CNBC_Utils.readCookie('c_cnbc_session_valid') == 'true' || CNBC_Utils.readData('c_to') || location.hash.indexOf('#sh=') != -1) {
			CNBC_Surf.Signin_Ui.startUIFlow('load');
		}else {
			//console.log('user not logged in, init CNBC_Premium');
			CNBC_Utils.appendClassesOnUserLoggedInOrPro();
			CNBC_Premium.init();
		}
	});
