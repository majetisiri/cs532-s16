            <script id="mps-gpt-shared" class="mps-script">
              
              // mps._setAdId(gptadid[string]) used to modify gptid in g.definedSlot()
              mps._setAdId = function(gptadid){
                if(typeof mps.transformAdId == 'function' && gptadid) {
                  var modgptadid = mps.transformAdId(gptadid).toString();
                  mps._debug('[mps:setAdId] modified gptadid: '+modgptadid);
                  return modgptadid;
                }else{
                  mps._debug('[mps:setAdId] same gptadid: '+gptadid);
                  return gptadid;
                }
      
              };
              // mps._traverseGPT(gptobj[object], lvl[number]=levels deep)
              // Find variables correlator if GPT api changes: Unused as of 10/13/15
              mps._traverseGPT = function(obj,lvl) {
                var isArray = obj instanceof Array;
                var isObject = obj instanceof Object;
                obj = obj || {}; lvl = lvl||0;
                if (!isArray && isObject) {
                  lvl++;
                  for (var k in obj) {
                    if (lvl < 3 && mps._keys(mps._smartgpt).length < 1) {
                      mps._traverseGPT(obj[k],lvl);
                    } else {
                      lvl=0,obj={};
                      return;
                    }
                  }
                } else if (!mps._smartgpt.correlator && /correlator=/.test(obj)) {
                    obj = obj.split('correlator=')[1].split('&')[0];
                    mps._smartgpt['correlator'] = obj;
                    return;
                }
              };
      
              mps._abcallback = function(error) {
                if(Object.keys && Object.keys(gpt).length === 0 || error) {
                  mps._gptError = true;
                  mps._debug('[mps:GPT] There was an error loading GPT, check if an ad blocker is enabled.');
                } else {
                  mps._gptError = false;
                  mps._debug('[mps:GPT] GPT is loaded correctly, no ad blocker is enabled.');
                }
              };
      
              mps._gptloadCallback = function(){
                if(mps._ext && mps._reqs && mps._reqs[mps._ext._set]){
                  mps._reqs[mps._ext._set]['gptready'] = mps._elapsed('',true);
                }
                for(avk in mps.advars) {if(typeof(mps.advars[avk])=='string' && typeof(gpt[mps.advars[avk]])=='object') mps.adobs.push(gpt[mps.advars[avk]])}
                mps._adobs[mps._loadset] = mps.adobs;
                mps._gptloaded = true;
                if(mps._queue.gptloadset[mps._loadset]) {
                  while(mps._queue.gptloadset[mps._loadset].length) {
                    mps._queue.gptloadset[mps._loadset].shift().call();
                  }
                }
                if(!mps._queue.gptloadset[mps._loadset]) {
                  mps._queue.gptloadset[mps._loadset] = []
                }
                if(mps._loadset === 0) {
                  if(mps._queue.mpsloaded.length) { mps._queue.mpsloaded.shift().call(); }
                }
                mps._debug('[mps:GPT] READY: gpt-asynchronous (processing '+mps._queue.gptloadset[mps._loadset].length+' queue items) '+mps._elapsed());
                if(typeof(mps.gptloadCallback)=='function') mps.gptloadCallback();
              };
      
              mps._adloadCallback = function(eo,loadset) {
                var _refreshed = 0;
                if(typeof(eo)=='object' && typeof(eo.slot)=='object') {
      
                  // SlotElementId by reference
                  var _adtarget = eo.slot.getSlotElementId();
      
                  // Create mps._adloads
                  var _adslot = _adtarget.replace('div-gpt-','').split('-');
                  var adslot = (_adslot[0]!='outofpage') ? _adslot[0] : '_oop';
                  var adload = (parseInt(_adslot[2]) > 0) ? parseInt(_adslot[2]) : 0;
                  if(mps._adloads[adload] && mps._adloads[adload][adslot]) _refreshed = 1;
                  mps._adloads[adload]=mps._adloads[adload]||{};
      
                  // AdUnitPath by reference
                  var _gptid =  eo.slot.getAdUnitPath();
      
                  // Create internal eo._mps object
                  eo._mps={'_elapsed':mps._elapsed('',true),'advar':mps._advars[adload][adslot],'adslot':mps._adslots[adload][adslot],'gptid':_gptid,'_slot':adslot,'_unit':mps._keys(mps.adunits,adslot).toString()};
                  if(adslot=='_oop') eo._mps._unit='outofpage';
      
                  // Correlator by reference
                  var gptcorrelator = googletag.pubads().getCorrelator();
                  if(gptcorrelator.length>0) eo._mps.correlator = gptcorrelator;
                  mps._debug("[mps:GPT] correlator: "+gptcorrelator+', '+adslot+': '+mps._elapsed('',true)+'ms');
      
      
                  var _adloads = _refreshed ? 'R'+adload : adload; // loaded v. refreshed
                  mps._adloads[_adloads] = mps._adloads[_adloads] || {};
                  mps._adloads[_adloads][adslot] = mps._adloads[_adloads][adslot] || [];
                  mps._correlator[_adloads] = mps._correlator[_adloads] || {};
                  mps._correlator[_adloads][adslot] = mps._correlator[_adloads][adslot] || [];
                  if(typeof adload == 'number' && adslot.length>0  && typeof adslot == 'string'){
                    if(_refreshed) mps._adloads[_adloads][adslot].push({'elapsed':mps._elapsed('',true),'correlator':eo._mps.correlator,'empty':eo.isEmpty});
                    else  mps._adloads[_adloads][adslot]=eo;
                    mps._correlator[_adloads][adslot] = eo._mps.correlator;
                  }
      
                  //save mps._req adslot end time
                  if(mps._reqs && mps._reqs[mps._loadset] && eo._mps._slot !== '_oop') mps._reqs[mps._ext._set]['end_'+mps._trim(eo._mps._slot,'_')] = mps._elapsed('',true);
                  mps._queue.render('adload', eo);
                  if(typeof(mps.adloadCallback)=='function') mps.adloadCallback(eo);
                  mps._debug("[mps:GPT] ADLOAD /"+eo._mps._slot+"/ "+(eo.isEmpty ? "(isEmpty) ":"")+mps._elapsed());
                            }
              };
      
              mps._execAd = function(slot,loadset,detect) {
                var loadset = (parseInt(loadset)>-1) ? parseInt(loadset) : mps._loadset;
                var gptdom;
                if(!mps._adslots || !mps._adslots[loadset]) {
                  mps._delays.execadready = mps._delays.execadready||250;
                  mps._debug('[mps:GPT] DELAYING mps._execAd: Not ready yet ('+slot+','+loadset+')');
                  setTimeout(function(){            mps._execAd(slot,loadset);
                   },mps._delays.execadready);            return false;
                }
                var gptdiv = mps._adslots[loadset][slot];
                if(!slot || !mps.adslots || !gptdiv || !(gptdom=document.getElementById(gptdiv))) {
                  mps._debug('[mps:GPT] FAILED mps._execAd');
                              return false;
                }
                if(mps._slotscalled[loadset][slot]) {
                  mps._debug('[mps:GPT] FAILED mps._execAd: Already called '+slot+' on loadset '+loadset);
                  return false;
                }
                          mps._slotscalled[loadset][slot] = gptdiv;
                if(mps.ab && typeof mps.ab._callback === 'function') { mps.ab._callback(slot, loadset);}
                googletag.cmd.push(function() {            mps._delays.gptdisplay = mps._delays.gptdisplay||0;
                  if(!parseInt(mps._delays.gptdisplay)) mps._delays.gptdisplay=0;
                  if(mps.slotsdisabled[loadset].indexOf(slot)==-1) {
                    setTimeout(function(){                mps._debug('[mps:GPT] googletag.display <'+loadset+':'+slot+'> '+mps._elapsed()); //DEBUG
                      googletag.display(gptdiv);
                    },mps._delays.gptdisplay);            } else {
                    mps._slotsdisabled[loadset][slot] = gptdiv;
                    delete mps._slotscalled[loadset][slot];
                    mps._classAdd(gptdom,'mps-gpt-hidden');
                    mps._debug('[mps:GPT] SKIP googletag.display <'+loadset+':'+slot+'> Responsive Disabled '+mps._elapsed()); //DEBUG
                  }
                });          return true;
              };
