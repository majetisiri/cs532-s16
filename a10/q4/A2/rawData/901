          window.speedTestResultTick = new Date().getTime()
        }
      }
    }());


    function decorateHostOrigin(fileName) {
        if (Array.prototype.indexOf) {
            var host = window.location.hostname;
            if (/oximity\.com/.test(host) && !/localhost/.test(host)) {
                fileName = "//d248prrhso0s59.cloudfront.net" + fileName;
            }
        }

        return fileName
    }

    function loadResource(fileName, fileType) {
        fileName = decorateHostOrigin(fileName);
        loadResourcePlain(fileName, fileType);
    }
    function loadResourcePlain(fileName, fileType) {
        if (fileType == "js") {
            document.write('<script type="text/javascript" src="');
            document.write(fileName);
            document.write('"></scr' + 'ipt>');
        } else if (fileType == "css") {
            document.write('<link rel="stylesheet" type="text/css" href="');
            document.write(fileName);
            document.write('">');
        }
    }

    /* debugging? */
