    (function() {
      function lowercase(text) {
        return text && text.toLowerCase() || '';
      }

      var n = Number((/android (\d+(\.\d+)?)/.exec(lowercase((window.navigator || {}).userAgent)) || [])[1]);
      if (n > 0 && n < 4.4) {
        document.write('<link rel="stylesheet" href="https://d248prrhso0s59.cloudfront.net/newsapp/ver/b9766/css/proxima-ttf.css">');
