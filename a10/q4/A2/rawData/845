         <script type="text/javascript">
            $(document).ready(function(){
            		if(window.location.href.indexOf("/category/") > -1) {
					    $('#iframe_ad_top').hide();
					}
					$(window).scroll(function(){
					if($(window).scrollTop()>150) {
					$("#header-sticky").slideDown().show();
					}else {
					$("#header-sticky").slideUp().hide('slow');
					}
					});
					/***** Search Box ******/
					if($(window).width() > 991) {
					  var devise = '#desktop-search';
					  var input_width = '200px';
					  var input_height = '30px';
					}else{
					  var devise = '#mobile-search';
					  var input_width = $("#header-mobile .container").width();	
					  var input_height = '50px';
					}
					  var search2 = $(devise+' #s');					  				 					  
					  if(search2.val() != '') {
						  $(search2).animate({ width: input_width, height : input_height}, 400).addClass("search-bg-remove");
						$(devise+' #searchsubmit').show('fast');						
					  }
					  
					  $(search2).on('focus', function(){					 	
						$(this).animate({ width: input_width, height : input_height}, 400).addClass("search-bg-remove");
						$(devise+' #searchsubmit').show('fast');	
						if($(devise+' #s').val() == '') {
							$(devise+' #searchsubmit').addClass("close-search");	
						}
					  });		
					  			  
					  $(search2).on('blur', function(){
						if(search2.val() == '') {
						  $(search2).animate({width: '30px', height : input_height}, 400).removeClass("search-bg-remove").addClass("search-bg");	
						  $(devise+' #searchsubmit').hide('fast').removeClass("close-search");			 
						}
					  });		
					  $(search2).keyup(function(){
						  if($(devise+' #s').val() == '') {
						 	$(devise+' #searchsubmit').addClass("close-search");
						  }else {
							 $(devise+' #searchsubmit').removeClass("close-search");
						  }
						
					  })				  
					  $(devise+' #searchform').submit(function () {
						var name = $(devise+' #s').val();				
						if (name === '') {
							return false;
						}
						});			  
					  /***** Search Box End ******/
				
					/********* Navigation Code ********/
					$(".menu-item-has-children").addClass("dropdown");
					$( "li.dropdown" ).find("a:first").attr({
					  "role" : "button",
					  "data-toggle" : "dropdown"
					}).addClass("parent-arrow");
					
					$(".navbar-toggle").click(function(){					
						if($(".collapsed").html())	{													
							$("#mb-nav").css({"display":"block","height":$(document).height()});
							$("#menu_button").removeClass("fa fa-bars").addClass("fa fa-close");
							//$("#header-mobile").css({"position":"absolute","top":$(document).scrollTop()});							
							$("#header-mobile").css({"position":"fixed","top":0,"height":$(document).height()-$(document).scrollTop(),"overflow-y":"scroll"});								
						}else {										
							$("#mb-nav").fadeOut(400);
							//$("#header-mobile").css({"position":"fixed","top":"0"});
							$("#header-mobile").css({"position":"fixed","top":"0","height":"auto","overflow":"visible"});
							$("#menu_button").removeClass().addClass("fa fa-bars");				
						}
					});
					
					/********* Navigation Code End ********/	
																									
                    if($('.entry-title').is(':empty'))
                    {
                        $('.article-index').hide();
                        $('.entry-title').hide();
                    }
                    var heights = $("div.ymla-post-box").map(function ()
                    {
                        return $(this).height();
                    }).get(),           
                    maxHeight = Math.max.apply(null, heights) + 10;

					/********** Discussion and Footer Code **********/
                   if($(window).width() < 991){
					    $('#disqus_comment_system').hide();
                    	$('.discusstion-tab').click(function(){						
                        $('#disqus_comment_system').slideToggle();
                        $('.discusstion-tab').toggleClass('showHide');  
                    	}).css({"cursor":"pointer"});
						
						$('#footer-slid').hide().css({"cursor":"pointer"});
                    	$('.footer-tab').click(function(){						
                        $('#footer-slid').slideToggle();
                        $('.footer-tab').toggleClass('f-showHide');  
                    	}).css({"cursor":"pointer"});												
					}
					/********** Discussion and Footer Code End **********/
					/********** Sticky Code **********/
					if($(window).width() > 991){
					  var popular_section_height = 0;
					  if($(".fixed-div2 ol li:lt(5) .popular-post-text").offset()){
					$(".fixed-div2 ol li:lt(5) .popular-post-text").each(function() {
					   popular_section_height += $(this).outerHeight(); 
					});
					  }else{
					popular_section_height = $(".fixed-div2").height()-60;
		
					  }
					$(".sidebar-1-stiky").css({"height": $(".fixed-div1").outerHeight()});
					$(".sidebar-2-stiky").css({"height":popular_section_height + 80});	
					var space = $(".left-sidebar-1-stiky").width() - 160;																					
					var leftSidebarImageMargin = space/2;
					$(window).scroll( function(){ 	
						if($(".left-sidebar-1-stiky").offset()){
						if($(window).scrollTop()>$(".left-sidebar-1-stiky").offset().top){
							$(".fixed-div3").css({"position":"fixed", "top":"0","margin-left": leftSidebarImageMargin+"px"});
						}else {
						$(".fixed-div3").css({"position":"inherit","top":"auto","margin-left":"auto"});
						}
						if($(window).scrollTop() > $(".report-outer").offset().top - 600) {
							$(".fixed-div3").css({"margin-left":"0","position":"relative","display":"block","top":$(".report-outer").offset().top - 600 - $(".left-sidebar-1-stiky").offset().top - 60});							
						}else {
						$(".fixed-div3").css({"display":"block"});
						}
						}
						if($(".sidebar-1-stiky").offset()){				      
							if($(window).scrollTop()>$(".sidebar-1-stiky").offset().top){		
							  $(".fixed-div1").css({"position":"fixed", "top":"0","z-index":"1", "width": $(".sidebar-1-stiky").width()});
							  $(".fixed-div2").css({"position":"fixed","display":"block", "top": $(".fixed-div1").height()+10});
							}else if($(window).scrollTop()<$(".sidebar-1-stiky").offset().top) { 
							  $(".fixed-div1").css({"position":"relative", "top":"auto","z-index":"1"});
							  $(".fixed-div2").css({"position":"relative","display":"block", "top":"auto"});			
							}
						   if($(window).scrollTop()>$(".footer").offset().top - ($(".sidebar-1-stiky").outerHeight() + $(".sidebar-2-stiky").height())){
							   //$(".fixed-div2").css({"display":"none"});						  
							   $(".fixed-div1").css({"position":"relative","display":"block","top":$(".footer").offset().top - $(".sidebar-1-stiky").outerHeight() - $(".sidebar-2-stiky").height() - $(".sidebar-1-stiky").offset().top});
							   
							   
							   $(".fixed-div2").css({"position":"relative","display":"block","top":$(".footer").offset().top - $(".sidebar-2-stiky").height() - $(".sidebar-2-stiky").offset().top + 15});
							}
						}
						
					})
					}
					/********** Sticky End **********/
				    $('iframe a').attr('target', '_blank');	
					for(var i = 1; i <= $(".tptn_posts_daily  .popular-post li").size(); i++){						
					$(".tptn_posts_daily  .popular-post li:nth-child("+i+") .popular-post-number" ).append(i);
					}
					
					$('.fa-times-circle-o').click(function(){
							$('#signup-box').animate({bottom:"-142px", opacity: 0}, 2000 );
							setCookie('bspopup','1');
						});	
						
						if(!checkCookie('bspopup')) {
							//setTimeout(showbottomsignup,20000);
							setCookie('bspopup','1');
						}
            });
			/* Right Bottom box */
			function showbottomsignup() {
				if($(window).width() > 768) {
					$('#signup-box').animate({bottom:"0px", opacity: 1}, 2000 ).css('position','fixed');
				}
			}						
			function setCookie(cname, cvalue) {
				document.cookie = cname + "=" + cvalue;
			}
			
			function getCookie(cname) {

				var name = cname + "=";
				var ca = document.cookie.split(';');
				for(var i=0; i<ca.length; i++) {
					var c = ca[i];
					while (c.charAt(0)==' ') c = c.substring(1);
					if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
				}
				return "";
			}
			
			function checkCookie() {
				var pcookie = getCookie("bspopup");
				if (pcookie != "") {
					return true;
				} else {
					return false;
				}
			}						
			/* Right Bottom box End */
           function iframeLoaded(idValue) 
           {
                var iFrameID        = document.getElementById(idValue);
                var custom_height   =   0;
                if(iFrameID && idValue.indexOf('iframe_ad_') != -1 && $('#'+idValue).length > 0)
                {
                    iFrameID.height = "";
                    if(idValue == "iframe_ad_content_bottom_ad")
                    {
                       custom_height    =   '56'; 
                    }
                    else if(idValue != "iframe_ad_content_bottom_ad")
                    {
                        custom_height   = iFrameID.contentWindow.document.body.scrollHeight;
                    }
                    
                    iFrameID.height = custom_height + "px";
                    if(idValue  ==  'iframe_ad_top')
                    {
                       if($('#bottom_advertisement_section').length > 0)
                        {
                            var bottom_section_value =   $('#bottom_advertisement_section').html();
                            if(bottom_section_value.indexOf(',') >  0)
                            {
                                $('#bottom_advertisement_section').html('')
                                var bottom_section_array =   bottom_section_value.split(',');
                                var request_type         =   bottom_section_array[0].trim();
                                var request_post_id      =   bottom_section_array[1].trim();
                                var exclude_post_id      =   $('#'+idValue).contents().find('#post_id_shown').val()
                                var replace_html_content =   '<iframe onload="iframeLoaded(\'iframe_ad_bottom\')" src="http://www.profitconfidential.com/advertisement-in-iframe/?type_of_advertisement='+request_type+'&advertisment_id='+request_post_id+'&exclude_post_id='+exclude_post_id+'" style="width:100%;border:none;" id="iframe_ad_bottom" scrolling="no"></iframe>';
                                $('#bottom_advertisement_section').append(replace_html_content);
                                $('#bottom_advertisement_section').show();
                            }
                        }
                    }
                }     
            }  
        </script>           
