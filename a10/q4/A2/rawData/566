<script type="text/javascript">

  if (typeof dj == "undefined") { var dj = Class.create({}); }
  if (typeof dj.blogs == "undefined") { dj.blogs = Class.create({}); }
  if (typeof dj.blogs.util == "undefined") { dj.blogs.util = Class.create({}); }

  dj.blogs.util.Animal = Class.create ( {
    init: function(name, sound) {
      this.name = name;
      this.sound = sound;
    },
    speak: function(divname) {
      $("#" + divname).html("");
      $("#" + divname).append("The " + this.name + " has " + this.sound + "ed!");
    }
  });



  function classTest(event){
    $("#myDivbark").html("The button was clicked!");
    $("#myDivbark").css({ 'color': 'red', 'font-size': '150%' });
    var dog = new dj.blogs.util.Animal('Dog', 'Bark');
    dog.speak("myDivbark");
  }

  $(document).ready( function () {
    $("#myButtonBark").click(classTest);
  });

</script>

