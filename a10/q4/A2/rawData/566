function count_display (social,count) {
    
    if(  count !== 0 && count <= 99 ){
         $('.' + social).children()[1].style.display = 'inline-block' ; 
       
       }else if(count > 99 && count <= 999 ) {
           $('.' + social).children()[1].style.display = 'inline-block' ; 
           $('.' + social).css("width","37px");
        }       
      
        else if(count > 999 ) {
           $('.' + social).children()[1].style.display = 'inline-block' ; 
           $('.' + social).css("width","43px");
        }

     }


