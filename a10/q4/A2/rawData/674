<script type="text/javascript" >

	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_909096").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/909096?view=json", function(json){
				jQuery("#upv_909096").empty();
				if (json.stats && json.stats.uniqueVisitors >= 0) {
					jQuery("#upv_909096").append(json.stats.uniqueVisitors);				
				} else {
					jQuery("#upv_909096").append("Not available");
				}
			});
		}
	}
</script>
			</div>
			
										
