			$('.teamPhotos li').removeClass('bounce');
		});
		
		$('.briefDesc img').addClass('imgBorder');
		

		// search feature
		var xhr = false;
		$("#SearchField").keyup(function() {
			var q = $(this).val();
			if (q.length > 1) {
				// allow search
				if (xhr !== false) {
					xhr.abort();
				}
				xhr = $.get('/full_search/q:' + encodeURI(q), function(data) {
					if (data.Location.length == 0 && data.Menu.length == 0 && data.Catering.length == 0) {
						$("#searchResults").html("<div>No Results</div>");
					}
					else {
						$("#searchResults").html("");
						if (data.Menu.length > 0) {
							var menuHTML = '';
							menuHTML += '<div>Menu</div>';
							menuHTML += '<ul>';
							for(var i in data.Menu) {
								menuHTML += '<li><a href="/food/menu/' + data.Menu[i].Product.slug + '">' + data.Menu[i].Product.name + '</a></li>';
							}
							menuHTML += '</ul>';
							$("#searchResults").append(menuHTML);
						}
						if (data.Catering.length > 0) {
							var catHTML = '';
							catHTML += '<div>Catering</div>';
							catHTML += '<ul>';
							for(var i in data.Catering) {
								catHTML += '<li><a href="/food/catering/' + data.Catering[i].Product.slug + '">' + data.Catering[i].Product.name + '</a></li>';
							}
							catHTML += '</ul>';
							$("#searchResults").append(catHTML);
						}
						if (data.Location.length > 0) {
							var locHTML = '';
							locHTML += '<div>Locations</div>';
							locHTML += '<ul>';
							for(var i in data.Location) {
								locHTML += '<li><a href="/locations/store/' + data.Location[i].Location.slug + '">' + data.Location[i].Location.title + '</a></li>';
							}
							locHTML += '</ul>';
							$("#searchResults").append(locHTML);
						}
					}
				}, "json");
			}
			else {
				$("#searchResults").html("");
			}
		});



		
		$('.slideContent').animate({left: '+=470'}, 200);
		$('.slideContent').css('width','480px');
				
		
		$('.slideContent .close').click(function(){
		    $('.slideContent').animate({left: '-=470'}, 200);
		    $('.slideContent').css('width','450px');
		});
		
		$('.hideJob').click(function(){
		    $('#jobOp').hide();
		});
		
