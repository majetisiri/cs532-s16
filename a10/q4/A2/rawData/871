			speed: 1000,
			pager:  '#pagerNav',
			pause: true,
			slides: '> *'
		});
		
		$('.locationsAlert').cycle({
			fx: 'scrollHorz',
			timeout: 3000,
			speed: 1000,
			slides: '> *'
		});		
		
		if ($('#pagerNav span').length == 1) {
		    $('#pagerNav').hide();
