	$('.nodeBox').toggle(function(){
		// store starting position
		var $currentSlide = $("div.slide.active");
		var curIndex = $("div.slide").index($currentSlide);
		var curID = $(this).attr("id");
		
		var forb = 'front';
		if ($currentSlide.hasClass('flipped')) {
			forb = 'back';
		}
		var $frontback = $currentSlide.find("." + forb);
		var $imageBack = $frontback.find(".imageBack");
		var $imageMid = $frontback.find(".imageMid");
		var $imageFore = $frontback.find(".imageFore");
		
		// cache original values
		originalPositions.imageBack.width = $imageBack.width();
		originalPositions.imageBack.height = $imageBack.height();
		originalPositions.imageBack.margin = $imageBack.css('margin');
		
		originalPositions.imageMid.width = $imageMid.width();
		originalPositions.imageMid.height = $imageMid.height();
		originalPositions.imageMid.margin = $imageMid.css('margin');
		
		originalPositions.imageFore.width = $imageFore.width();
		originalPositions.imageFore.height = $imageFore.height();
		originalPositions.imageFore.margin = $imageFore.css('margin');
		
		var nodeValues = slideNodes[curIndex][forb][curID];
		var easeInTime = 1000;
		var easeInType = "easeInOutQuad";
		
	    $(".imageBack").animate({
	    	"width" : nodeValues.imageBack.width,
	    	"height" : nodeValues.imageBack.height,
	    	"margin" : nodeValues.imageBack.margin
	    }, easeInTime, easeInType);
	    $(".imageMid").animate({
	    	"width" : nodeValues.imageMid.width,
	    	"height" : nodeValues.imageMid.height,
	    	"margin" : nodeValues.imageMid.margin
	    }, easeInTime, easeInType);
	    $(".imageFore").animate({
	    	"width" : nodeValues.imageFore.width,
	    	"height" : nodeValues.imageFore.height,
	    	"margin" : nodeValues.imageFore.margin
	    }, easeInTime, easeInType);
	}, 
	function() {
		var easeOutTime = 500;
		var easeOutType = "easeOutQuad";
	
		$(".imageBack").animate({
	    	"width" : originalPositions.imageBack.width,
	    	"height" : originalPositions.imageBack.height,
	    	"margin" : originalPositions.imageBack.margin
	    }, easeOutTime, easeOutType);
	    $(".imageMid").animate({
	    	"width" : originalPositions.imageMid.width,
	    	"height" : originalPositions.imageMid.height,
	    	"margin" : originalPositions.imageMid.margin
	    }, easeOutTime, easeOutType);
	    $(".imageFore").animate({
	    	"width" : originalPositions.imageFore.width,
	    	"height" : originalPositions.imageFore.height,
	    	"margin" : originalPositions.imageFore.margin
	    }, easeOutTime, easeOutType);
	});
	
			
});
	
</script>		



