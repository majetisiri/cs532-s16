	function randOrd() {
    return (Math.round(Math.random())-0.5); 
    }
    
    function randomValue(arr) {
	    var count = arr.length;
	    var i = Math.floor(Math.random() * count);
	    return arr[i];
    }

	
	$(document).ready(function(){
		
		var badBrowsers = {
			"safari" : 531.22,
			"mozilla" : 1.9,
			"msie" : 8
		};
		
		for(var i in badBrowsers) {
			if (typeof $.browser[i] != 'undefined' && parseFloat($.browser.version) < badBrowsers[i]) {
				$(".browserUpdate").show();
			}
		}
		
		$(".browserUpdate .dismiss").click(function() {
			$(".browserUpdate").hide();
		});

		$("#logo").hover(
			function () {
				$(this).attr('class', 'animated swing');
			},
			function () {
				$(this).removeClass('animated swing');
			}
		);
		
		$(".blogEventsWrap ul li").hover(
			function () {
				$(this).addClass('animated swing');
			},
			function () {
				$(this).removeClass('animated swing');
			}
		);

		
		
		$('.home #logo').addClass("animated bounceInDown");

	
		$('.cycle').cycle({
			fx: 'fade',
