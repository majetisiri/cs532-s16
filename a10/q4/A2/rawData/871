	$(document).ready(function(){
	
		$(".cycle").cycle({
			timeout: 0,
			speed: 1000,
			fx: "scrollHorz",
			prev: ".prev",
			next: ".next",
			after: function(el, next_el) {
		        $(next_el).addClass('active');
		    },
		    before: function(el) {
		        $(el).removeClass('active');
		    }
		});
		
		$("#parallax1a").smart3d({
			vertical: false,
			invertHorizontal: true
		});
		
		$("#parallax1b").smart3d({
			vertical: false,
			invertHorizontal: true
		});
		
		$("#parallax2a").smart3d({
			vertical: false,
			invertHorizontal: true
		});
		
		$("#parallax2b").smart3d({
			vertical: false,
			invertHorizontal: true
		});
		
		$("#parallax3a").smart3d({
			vertical: false,
			invertHorizontal: true
		});
		
		$("#parallax3b").smart3d({
			vertical: false,
			invertHorizontal: true
		});
		
		$(".flip").click(function(){
			var $active = $("div.slide.active");
			$active.toggleClass("flipped");
			if (typeof $.browser.webkit == 'undefined' || $.browser.webkit === false) { 
				$active.toggleClass('noWebKit');
				$active.find(".front").fadeToggle();
			}
		});
			
	
	$('.cycleContainer .nodeBox').toggle(function(){
	    $('.tooltip').hide();
	    $('.nodeBox').removeClass('topMost').hide();
	    $(this).show();
	    $('.node').removeClass('active');
	    $(this).find('.tooltip').fadeIn();
	    $(this).find('.node').addClass('active');
	    $(this).addClass('topMost');
	    $('.flip').fadeOut();
	    $('.prev').fadeOut();
	    $('.next').fadeOut();
	    
	}, 
	function(){
	    $('.tooltip').hide();
	    $('.nodeBox').show();
	    $('.flip').fadeIn();
	    $('.prev').fadeIn();
	    $('.next').fadeIn();
	});
	
	var originalPositions = { "imageBack" : { "width" : 0, "height" : 0, "margin" : "" }, "imageMid" : { "width" : 0, "height" : 0, "margin" : "" }, "imageFore" : { "width" : 0, "height" : 0, "margin" : "" } };
	var slideNodes = {
		"0" : {
			"front" : {
				"node1a-1" : {
					"imageBack" : {
						"width" : 2177,
						"height" : 990,
						"margin" : "-370px 0 0 -420px"
					},
					"imageMid" : {
						"width" : 2177,
						"height" : 990,
						"margin" : "-320px 0 0 -420px"
					},
					"imageFore" : {
