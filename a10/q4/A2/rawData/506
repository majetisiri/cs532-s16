<script>
$.fn.vAlign = function() {
  return this.each(function(i) {
    var thisHeight = $(this).height();
    var parentHeight = $(this).parent().height();
    var marginHeight = Math.ceil((parentHeight-thisHeight) / 2);
    $(this).css('margin-top', marginHeight);
  });
};
$(document).ready(function(){
  $('.content').vAlign(); //vAlign the message and cta button.
    $.ajax({
       url: "/trackings",
       type: 'POST',
       data: {tracking:{link_id:"56b5232c3066320003830700"}},
       dataType : 'json',
       timeout: 2000,
       success: function(data) {
          $(".call_to_action").click(function(e) {
            e.preventDefault();
            var href = $(this).attr("href");
            $.ajax({
               url: "/trackings/"+data.id,
               type: 'PUT',
               data: {tracking:{clicked:1}},
               dataType : 'json',
               timeout: 2000,
               complete: function() {
                  window.location = href;
               }
            });
          });
        }
      });
  $(window).on('resize', function() {
    $('.content').vAlign(); //vAlign the message and cta button on resize.
  });
  $(window).load(function() {
   // executes when complete page is fully loaded, including all frames, objects and images
    msieversion();
  });
  var iFrameID = document.getElementById('iframe').contentWindow.document;
  var iOS = ( navigator.userAgent.match(/(iPad|iPhone|iPod)/g) ? true : false );
  if (iOS)
  {
    iFrameID.addClass('ios');
  } else {
    $('#iframe').attr("scrolling", "yes");
      $('#iframe').attr("src", $('#iframe').attr("src"));
  }

});


</script>


