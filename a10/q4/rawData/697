<script type="text/javascript">
function addSticky(selector,stickyClass){
    // if (selector.data('sticked')) return; // one-time sticky
    var h = selector.outerHeight();
    selector.addClass(stickyClass);
    selector.next().css('margin-top',h);
    selector.data('sticked', true);
}
function removeSticky(selector,stickyClass){
    selector.removeClass(stickyClass);
    selector.next().css('margin-top',0);
}
function removeAllSticky(){
    removeSticky($("#adunit_article_center_top_computer"),"sticky2");
    removeSticky($("#adunit_article_right_top_computer"),"sticky");
    removeSticky($("#adunit_article_right_middle1_computer"),"sticky");
    removeSticky($("#adunit_article_right_middle2_computer"),"sticky");
    removeSticky($("#adunit_article_right_middle3_computer"),"sticky");
}
var sticky1 = new Waypoint({
    element: $("#adunit_article_center_top_computer"),
    handler: function(direction) {
        if(direction==='down'){
            addSticky($("#adunit_article_center_top_computer"),"sticky2");
        }
    },
    offset:function(){
        return 52;
    }
});
var sticky01 = new Waypoint({
    element: $(".contents").first(),
    handler: function(direction) {
        if(direction==='up'){
            removeAllSticky();
        }
    },
});
var sticky2 = new Waypoint({
    element: $("#adunit_article_right_top_computer"),
    handler: function(direction) {
        if(direction==='down'){
            removeAllSticky();
        }
    },
    offset:function(){
        return $("#adunit_article_center_top_computer").outerHeight();
    }
});
