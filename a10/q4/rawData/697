        <script>
			sliderInt = 1;
			sliderNext = 2;

			$('.photo_slide').fadeOut(-100);
			$('#photo1').fadeIn(300);

			$(document).ready(function(){
				count = $('.slide img').size();

				$('.current').html(1);

			});

			function prev(){
				newSlide = sliderInt -1;
				showSlide(newSlide);

			}

			function next(){
				newSlide = sliderInt +1;
				showSlide(newSlide);
			}

			function showSlide(Id){

			if(Id > count){
				Id=1;
			}else if(Id<1){
				Id =count;
			}
			$('.photo_slide').fadeOut(0);
			$('#photo'+Id).fadeIn(300);
			console.debug(Id);

			sliderInt = Id;
			sliderNext = Id +1;
			$('.current').html(Id);

			}

		</script>
