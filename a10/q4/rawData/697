<script type="text/javascript">
$('input[type="button"]').click(function(){
	if($(this).val() == 'Subscribe'){$.ajax({url:'/home/news/newsletter/mailchimp_insert.php',type:'post',data:{email:$('#nmail').val()}}).done(function(e){  $('#rword2').html(e); });}
});
if(!$.cookie('nl_popup')){
	$('.wp-nwletter-popup').show();
	$.cookie('nl_popup', '1', { path: '/', domain: 'techtimes.com', expires: 1 });
	$('html').on("click",function() { $('.wp-nwletter-popup').hide(); });
	$('.con-popup').click(function(event){ event.stopPropagation(); });
	$('.popup-cls').click(function(){ $('.wp-nwletter-popup').hide(); return false; })
}
	$("#donotshow").click(function(){
		$.cookie('nl_popup', '1', { path: '/', domain: 'techtimes.com', expires: 1 });
		$('.wp-nwletter-popup').hide(); return false;
	});

</script>
</body></html>