<script type='text/javascript'>


Reuters.YoplayerStatus = {
   state: false,
   isReady: false,
   videoPlayed_ : false,
   videoPlayEnded_ : false,
   videoPlayPaused_:false,
   currentPlayTime: 0
}


var rcom_vps = Object.create(Reuters.YoplayerStatus); //rcom video player status

function artCbVideoStrategy(player) {
  this.player_ = player;
  this.ready_ = false;
  this.currentAdPosition_ = undefined;
  this.videoPlayed_ = false;
  this.videoPlayEnded_ = false;
  this.videoPlayPaused_ = false;
  this.viewStartTime_ = new Date().getTime(); 
  this.videoStartTime_ = undefined;
}


artCbVideoStrategy.ContentType = {
  AD: 'ad',
  CONTENT: 'ct'
};


artCbVideoStrategy.AdPosition = {
  PREROLL: 'a1',
  MIDROLL: 'a2',
  POSTROLL:'a3',
  OVERLAY: 'a4',
  SPECIAL: 'a5'
};


artCbVideoStrategy.VideoState = {
  UNPLAYED: 's1',
  PLAYED: 's2',
  STOPPED: 's3',
  COMPLETED: 's4'
};


artCbVideoStrategy.prototype.isReady = function() {
  this.viewStartTime_ = new Date().getTime(); 
  return rcom_vps.isReady; 
};


artCbVideoStrategy.prototype.getTitle = function() { 
	return Reuters.yovideo.title; 
};


artCbVideoStrategy.prototype.getVideoPath = function() { 
     return Reuters.yovideo.rcomvid; 
}; 


artCbVideoStrategy.prototype.getContentType = function() { 
  return "ct"; 
};


artCbVideoStrategy.prototype.getAdPosition = function() { 
  return  ''; 
};


artCbVideoStrategy.prototype.getTotalDuration = function() { 
    return this.getTimeInSeconds_(Reuters.yovideo.durationInSec);
};


artCbVideoStrategy.prototype.getState = function() { 
  if ( rcom_vps.videoPlayPaused_ ) {
    return artCbVideoStrategy.VideoState.STOPPED;
  }

  if( rcom_vps.videoPlayEnded ) {
    return artCbVideoStrategy.VideoState.COMPLETED;
  }

  if( ! rcom_vps.videoPlayed_ ) {
    return artCbVideoStrategy.VideoState.UNPLAYED;
  }
 
  return artCbVideoStrategy.VideoState.PLAYED;
};


artCbVideoStrategy.prototype.getCurrentPlayTime = function() { 
  var pt =  rcom_vps.currentPlayTime;
  return this.getTimeInSeconds_(pt); 
};


artCbVideoStrategy.prototype.getBitrate = function() { return 1240;};


artCbVideoStrategy.prototype.getThumbnailPath = function() { 
     return Reuters.yovideo.thumb; 
};

artCbVideoStrategy.prototype.getPlayerType = function() { 
   return "yospace";
};



artCbVideoStrategy.prototype.timeElapsed_ = function(timestamp) {
  if (timestamp === undefined) {
    return 0;
  }
  var t = new Date().getTime();
  return t - timestamp;
};


artCbVideoStrategy.prototype.getViewStartTime = function() { 
  if (isNaN(this.viewStartTime_)) {
    return 0;
  }

  return this.timeElapsed_(this.viewStartTime_);
};


artCbVideoStrategy.prototype.getViewPlayTime = function() { 
  if ( rcom_vps.videoPlayed_ ) {
    return this.timeElapsed_(this.viewStartTime_);
  }
  return -1;
};


artCbVideoStrategy.prototype.getViewAdPlayTime = function() { 
  return -1;
};


artCbVideoStrategy.prototype.getTimeInSeconds_ = function(t) {
  return ((t === -1 || isNaN(t)) ? -1 : t * 1000);
};

artCbVideoStrategy.verify = function(player) {
  return true;
};


  window['_cbv_strategies'] = window['_cbv_strategies'] || [];
  window['_cbv_strategies'].push(artCbVideoStrategy);
      
  var cbv_player = null;
  var _cbv = window._cbv || (window._cbv = []);
  _cbv.push(cbv_player);

</script>

</div>

