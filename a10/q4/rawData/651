<script type="text/javascript">
function drawBreakingNews(res) {
	
    if (typeof res === 'undefined' || $.isEmptyObject(res) || isClosedBreakingNews(res)) {
        return;
    }

    if (res.type !== '') {
        var typeKey = "breakingNews." + res.type;
        var typeStr = '' + res.label + ':';
        $('.breaking-news-type').text(typeStr);
    }
    var typeClass = res.type;
    if (typeClass == '') {
        typeClass = 'empty happening';
        var typeText = 'HAPPENING NOW:';
        $('.breaking-news-type').text(typeText);
    }
    if (typeClass == 'developing'){
        $('#breakingNews').addClass('happening');
    }
    $('#breakingNewsContainer').addClass(typeClass);
    var breakingNewsContent = '';
    if (res.url != '') {
        breakingNewsContent = $('<a>').attr('href', res.url).html(res.headline);
    } else {
        breakingNewsContent = res.headline;
    }
    $('.breaking-news-content').html(res.headline);
    if (res.url != '') {
        var linkTxt = 'VIEW MORE';
        if( res.actionCall && res.actionCall != '' ){
        	linkTxt = res.actionCall;
        }
        var link = $('<a>').attr({href: res.url, id:'breaking-news-link'}).html(linkTxt);
        $('.breaking-news-content').append(link);
    }
    $('#breakingNewsContainer').data('breakingNewsJson', res);
    $('#breakingNewsContainer').removeClass('hidden');
    $('#headerMain').css( "padding-top","30px" );
    $('.edition-header').css( "padding-top","30px" );
    
    $('.edition-selector, .edition-label').click(function () {
        setTimeout(function () {
            if (($('.edition-container').hasClass('active')) && ($('.edition-container').css('padding-top') !== '30px') && (!$('#breakingNewsContainer').hasClass('hidden'))) {
                $('.edition-container').css('padding-top', '30px');
            } else if (!$('.edition-container').hasClass('active')) {
                $('.edition-container').css('padding-top', '');
            }
        }, 1)
    });
    $(window).scroll(function () {
        if (!$('#breakingNewsContainer').hasClass('hidden')) {
            if (($('#headerNav').hasClass('fixed')) && ($('#headerNav').css('top') !== '29px')) {
                $('#headerNav').css('top', '29px');
            } else if (!$('#headerNav').hasClass('fixed')) {
                $('#headerNav').css('top', '');
            }
        }
        if (($('.nav-white-space').hasClass('fixed')) && (!$('#breakingNewsContainer').hasClass('hidden'))) {
            $('.nav-white-space').css('padding-top', '30px');
        } else if (!$('.nav-white-space').hasClass('fixed')) {
            $('.nav-white-space').css('padding-top', '');
        }
    });
    $('#breakingNewsContainer .close-btn').click(function () {
        $('#headerMain').css('padding-top', '');
        $('.edition-container').css('padding-top', '');
        $('.edition-header').css('padding-top', '');
        $('#headerNav').css('top', '');
        $('.nav-white-space').css('padding-top', '');
    });
}

function isClosedBreakingNews(obj) {
    var flag = false;
    var lastCloseNews = Reuters.utils.getFromLocalStorage('lastclose.breakingnews');
    if (lastCloseNews) {
        if (obj.headline === lastCloseNews.headline 
                && obj.url === lastCloseNews.url && obj.type === lastCloseNews.type) {
            flag = true;
        }
    }
    return flag;
}

function saveClosedBreakingNews(obj) {
    Reuters.utils.setToLocalStorage('lastclose.breakingnews', obj);
}

function linkCloseBreakingNews() {
    var breakingNewsObj = $('#breakingNewsContainer').data('breakingNewsJson');
    saveClosedBreakingNews(breakingNewsObj);
}

$(function() {
    $('.close-btn').on('click', function() {
        $('#breakingNewsContainer').addClass('hidden');
        var breakingNewsObj = $('#breakingNewsContainer').data('breakingNewsJson');
        saveClosedBreakingNews(breakingNewsObj);
    });
    Reuters.utils.loadScript("breakingNewsAjax","/assets/breakingNews?view=json&sp=drawBreakingNews");

});
</script>
