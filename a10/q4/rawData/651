<script>
if (!(/iPad/i.test(navigator.userAgent))) {
  var shareArtTopShare = $('.column1.col.col-10 .reuters-share').first().offset().top + 30;
  var shareArtBottShare = $('.column1.col.col-10 .reuters-share').last().offset().top - 400;
  $(window).scroll(function () {
    if ($('.reuters-share.reuters-share-sticky').length > 0) {
      if (($(window).scrollTop() > shareArtTopShare) && (shareArtBottShare > $(window).scrollTop())) {
        $('.reuters-share-sticky').fadeIn(300);
        $('.column1.col.col-10 .reuters-share').last().css('visibility', 'hidden');
      }
      if (($(window).scrollTop() > shareArtBottShare) || (shareArtTopShare > $(window).scrollTop())) {
        $('.reuters-share-sticky').fadeOut(300);
        $('.column1.col.col-10 .reuters-share').last().css('visibility', 'visible');
      }
    }
  });
}
</script>   


<!-- GA Article Tracking -->
<script>
	(function switcher($) {
	 
		function init() {
	        events();
	    }

	    function events() {
	    	articleTracking();
	    }
        
		function articleTracking() {
			setTimeout(function() {
				var $relatedCoverageLinks = $('.related-news a'),
			    	$relatedVideoLinks    = $('.related-video a'),
			    	$moreFromReutersLinks = $('#taboola-bmc a'),
			    	$trendingLinks        = $('#most-popular-parsely li a');

				$relatedCoverageLinks.on('click',function() {
					var label = this.text.trim();	
					if (label != '') {
						dataLayer.push({
					        'analyticsAttributes': ''
					    });
					    dataLayer.push({
					        'event': 'GA_event',
					        'category': 'Click',
					        'action': 'Related Coverage',
					        'label': label,
					        'analyticsAttributes': {
					            'relatedCoverageClick': '1'
					        }
					    });
					}
				});

				$relatedVideoLinks.on('click',function() {
					var label = this.text.trim();	
					if (label != '') {
						dataLayer.push({
					        'analyticsAttributes': ''
					    });
					    dataLayer.push({
					        'event': 'GA_event',
					        'category': 'Click',
					        'action': 'Related Video',
					        'label': label,
					        'analyticsAttributes': {
					            'relatedVideoClick': '1'
					        }
					    });
					}
				});

				$moreFromReutersLinks.on('click',function() {
					var label = this.text.trim();	
					if (label != '') {
						dataLayer.push({
					        'analyticsAttributes': ''
					    });
					    dataLayer.push({
					        'event': 'GA_event',
					        'category': 'Click',
					        'action': 'More From Reuters',
					        'label': label,
					        'analyticsAttributes': {
					            'moreFromReutersClick': '1'
					        }
					    });
					}
				});

				$trendingLinks.on('click',function() {
					var label = this.text.trim();	
					if (label != '') {
						dataLayer.push({
					        'analyticsAttributes': ''
					    });
					    dataLayer.push({
					        'event': 'GA_event',
					        'category': 'Click',
					        'action': 'Trending On Reuters',
					        'label': label,
					        'analyticsAttributes': {
					            'trendingOnReutersClick': '1'
					        }
					    });
					}
				});
			}, 1000);
	    }

	    init();
	})(jQuery);
</script>  <script language="javascript" type="text/javascript" src="http://www.reuters.com/reuters_gpt_bootstrap_footer.js"></script>


