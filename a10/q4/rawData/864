						<input type="checkbox" name="agree" id="cc_agree" class="checkbox_input"/>
						<label for="cc_agree">
						{{tmpl "#TermsCopyTmpl"}}
						</label>
						<div class="paymentFieldError agreeFieldError"></div>
					</div>
				</div>
			{{/if}}
			<div class="subscription_terms payment_buttons payment_field_validated clearfix">

				{{if showPayButton}}
