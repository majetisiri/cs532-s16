	function onJWPlayerEvent( event, player, event_args ){
		DTEvent.trigger( 'jwplayer:' + event, player, event_args );
	}
	</script>
		<script>

		DTEvent.on('jwplayer:onFirstFrame', function(_player) {

			if(_player.attachment_id) {

				var req = $.ajax({
					url: ajaxurl + '?action=dt_video_view_tracker_ajax',
					cache: false,
					type: 'POST',
					data: {
						attachment_id: _player.attachment_id
					}
				}).done(function(res){

					if (res.status == 'OK' ){
						DT.Logger.log(res.message);

					}
					if (res.status == 'ERROR') {
						DT.Logger.log(res.message);
					}

				})
				.fail(function() {
					DT.Logger.log('DTV: Tracking request failed');
				});

				//undefined so there isn't a call on the next video within the player
				_player.attachment_id = undefined;

			}
			else {
				DT.Logger.log('DTV: No attachment id to track.');
			}

		});

	</script>
