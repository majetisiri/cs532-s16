	function submitform(obj)
	{	// check function is exist or not
		ih_js("#emessage").css({display:"none"});
		if(ih_js("#joinform #termschk").is(":checked")){
			var username = ih_js("#YumPopupregisterForm_username").val();
			var email = ih_js("#YumProfile_email").val();
			var password = ih_js("#YumPopupregisterForm_password").val();
			ih_js.ajax({
					url:'/popupregister',
					type: 'POST',
					data: {'YumPopupregisterForm[username]': ih_js.trim(username), 'YumProfile[email]': ih_js.trim(email), 'YumPopupregisterForm[password]': password},
					dataType: 'json',
					success: function(response){
						if(response.success==1){
							ih_js(window).error(function(e){
								e.preventDefault();
							});
							try{
								if (typeof validateoffers == 'function') { validateoffers();
									 setTimeout(function(){ window.location.href = 'http://investorshangout.com'+response.url; }, 2000);
								}else{
									window.location.href = 'http://investorshangout.com'+response.url;
								}
							}catch(e){}
							//window.location.href = ''+response.url;
						}else{ // show error message
							ih_js("#emessage").html(response.error);
							ih_js("#emessage").css({display:"block"});
						}
					}
			});
		}else{
			return false;
		}
	}
	function submitpaypalform(obj)
	{	// check function is exist or not
		//alert("abcd");
		ih_js("#emessage").css({display:"none"});
		if(ih_js("#joinform #termschk").is(":checked")){
			var username = ih_js("#YumPopupregisterForm_username").val();
			var email = ih_js("#YumProfile_email").val();
			var password = ih_js("#YumPopupregisterForm_password").val();
			if (document.getElementById('month_plan').checked)
				{
				  plan = document.getElementById('month_plan').value;
				}
			if (document.getElementById('year_plan').checked) 
				{
				  plan = document.getElementById('year_plan').value;
				}
			if(plan==1)
			{
				var hosted_button_id='XRCNCV2M5K9VW';
				var cmd='_s-xclick';
			}
			if(plan==2)
			{
				var hosted_button_id='YTA9PT23ZXJP8';
				var cmd='_s-xclick';
			}
			ih_js.ajax({
					url:'/popupregisterpaypal',
					type: 'POST',
					data: {'YumPopupregisterForm[username]': ih_js.trim(username), 'YumProfile[email]': ih_js.trim(email), 'YumPopupregisterForm[password]': password, 'hosted_button_id':hosted_button_id, 'cmd':cmd},
					dataType: 'json',
					crossDomain : true,
					success: function(response){
						if(response.success==1){
							ih_js(window).error(function(e){
								e.preventDefault();
							});
							try{
								if (typeof validateoffers == 'function') { validateoffers();
								setTimeout(function(){  window.location.href=response.url;},2000);
								}else{
									window.location.href=response.url;
								}
							}catch(e){}
							//window.location.href = ''+response.url;
						}else{ // show error message
							ih_js("#emessage").html(response.error);
							ih_js("#emessage").css({display:"block"});
						}
					}
			});
		}else{
			return false;
		}
	}
	function displaypaymentenable(obj)
	{
		ih_js("#payment_div").css({display:"block"});
		ih_js("#registration_container").removeClass("registration_border1");
		ih_js("#registration_container").addClass("registration_paypal");
		
		
	}
