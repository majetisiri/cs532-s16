                                resources: item.resources,
                                contentMetadata: {
   "originRID" : "10XKCTJSWV79ESJ61K8W",
   "browseNode" : null,
   "pageType" : "GoldBox",
   "widgetID" : "2449568062",
   "deviceType" : "pc",
   "reftag" : "as_li_ss_tl",
   "subPageType" : "main",
   "slotName" : "slot-3"
}
                            });
