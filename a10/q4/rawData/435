    var imageURL = gb.baseUtils.checkAndSetSSLImageUrl(gb.resources.getImage('goldbox_upcoming'));

    if(data.asin) {
        imageURL = data.asin.imageURL;
    } else {
        if(data.deal.detail.imageAsin) {
            imageURL = data.deal.detail.imageAsin;
        }

        if(data.deal.dealState === gb.enums.DealStates.UPCOMING || data.deal.dealState === gb.enums.DealStates.COMINGSOON) {
            if(data.deal.teaser.teaserImage) {
                imageURL = data.deal.teaser.teaserImage;
            }
        }
    }

    imageURL = gb.utils.resizeImage(imageURL, data.dealViewConfig.miniDPImageSize);
~>
    <div class="a-section miniDPImageBlock">
