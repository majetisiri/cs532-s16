    if(window.P && window.P.AUI_BUILD_DATE) {
        P.when("A", "a-popover", "ready").execute(function(A, popover) {
            var instance = popover.create(A.$("#subscription_header"), {
                content     : "<div class='inAuiSubscriptionPopover'>"+ A.$('#subscription_content').html() + "</div>",
                position    : "triggerBottom",
                width       : 340,
                closeButton : false
            });
        });
    } else if (window.amznJQ) {
        amznJQ.onReady('jQuery', function() {
            jQuery('#subscription_header').amazonPopoverTrigger({
                showOnHover: true,
                showCloseButton: false,
                hoverShowDelay: 100,
                hoverHideDelay: 100,
                width: 340,
                localContent: "#subscription_content",
                location: "bottom",
                locationAlign: "right"
            });
        });
    }
</script>


