var wps_disabled = false;
function wp_subscribe_popup() {
	if (!wps_disabled && !jQuery.cookie('wps_cookie_1460061764')) {
		jQuery.magnificPopup.open({
			items: {
		  		src: '#wp_subscribe_popup',
		  		type: 'inline'
			},
			removalDelay: 800,
			callbacks: {
			    beforeOpen: function() {
			       this.st.mainClass = 'animated zoomIn';
			    },
			    beforeClose: function() {
			    	var $wrap = this.wrap,
			    		$bg = $wrap.prev(),
			    		$mfp = $wrap.add($bg);

			    	$mfp.removeClass('zoomIn').addClass('zoomOut');
			    },
			  },
		});
		jQuery.cookie('wps_cookie_1460061764', '1', { path: '/', expires: 7});
		wps_disabled = true;
	}
}
jQuery(window).load(function() {
	wp_subscribe_popup();
	if (jQuery('#wp-subscribe-content-bottom').length) {
		var content_bottom = Math.floor(jQuery('#wp-subscribe-content-bottom').offset().top);
		jQuery(window).scroll(function(event) {
			var viewport_bottom = jQuery(window).scrollTop() + jQuery(window).height();
			if (viewport_bottom >= content_bottom) wp_subscribe_popup();
		});
	}
});

jQuery(document).ready(function($) {
	setTimeout(wp_subscribe_popup, 5000);
	$(document).exitIntent(wp_subscribe_popup);
});
</script>
