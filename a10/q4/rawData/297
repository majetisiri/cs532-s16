		function AnvPlaylist(divId,playlist,playerInstance,playlistHeight,numVideos) {
		console.log("@AnvPlaylist playlist:",playlist);
		var pHeight = playlistHeight;
		if(playlist.length < 2)
		{
			pHeight = 0;	
		}
		var container=document.getElementById(divId);
		container.style.height= pHeight + "px";
		container.style.overflowX="hidden";
		container.style.overflowY="scroll";
		var row;
		var addedRow=false;
		for(var i=0, len=playlist.length; i<len; i++) {
			if(i < numVideos) {
				var entry=playlist[i];
				var item=document.createElement("div");
				var thumb=document.createElement("img");
				var play=document.createElement("div");
				var title=document.createElement("div");
				var header=document.createElement("div");
				header.style.position="absolute";
				header.className="anv-item-header";
				header.style.top="0";
				header.style.height="auto";
				header.style.width="100%";
				header.style.padding="0px";
				item.id="anv-item"+i;
				item.style.position="relative";
				item.className="anv-item span7";
				item.style.marginBottom="4px";
				item.style.color="white";
				item.style.display="none";
				item.onclick=onItemSelected;
				item.style.cursor="pointer";
				thumb.onload=onImageLoad;
				thumb.src=entry.src_image_url;
				thumb.className="full-wide";
				play.style.background="url('http://media.graytvinc.com/designimages/anv_2_sprite32.png') 0 -18px";
				play.style.width="36px";
				play.style.height="36px";
				play.style.backgroundSize="400%";
				play.style.display="block";
				title.style.fontSize="12px";
				title.style.letterSpacing="0.02em";
				title.style.lineHeight="1.4em";
				title.style.position="relative";
				title.style.whiteSpace="wrap";
				title.style.fontFamily="Arial, sans-serif";
				title.style.padding="4px";
				title.innerHTML=entry.def_title;
				title.style.maxHeight="30px";
				title.style.overflow="hidden";
				item.appendChild(thumb);
				header.appendChild(title);
				header.appendChild(play);
				item.appendChild(header);
				if(i == 0 || (i % 2) == 0) {
					addedRow = false;
					row=document.createElement("div");
					row.className="row-fluid";
					row.appendChild(item);
				} else {
					addedRow = true;
					row.appendChild(item);
					container.appendChild(row);
				}
				console.log("language",entry.language);
			}
		}
		if(addedRow == false) {
			container.appendChild(row);
		}
		
		function onImageLoad() {
			this.parentNode.style.display="block";
		}
		
		function onItemSelected() {
			pat=/anv-item(.+)/;
			var m=pat.exec(this.id);
			var index=parseInt(m[1]);
			playerInstance.play(playlist[index]);
			console.log(index);
		}
	}
	function AnvPlaylistList(divId,playlist,playerInstance,playlistHeight,numVideos) {
		console.log("@AnvPlaylist playlist:",playlist);
		var pHeight = playlistHeight;
		if(playlist.length < 2)
		{
			pHeight = 0;	
		}
		var container=document.getElementById(divId);
		container.style.height= pHeight + "px";
		container.style.overflowX="hidden";
		container.style.overflowY="scroll";
		var row;
		var addedRow=false;
		for(var i=0, len=playlist.length; i<len; i++) {
			if(i < numVideos) {
				var entry=playlist[i];
				var item=document.createElement("div");
				var itemContainer=document.createElement("div");
				var thumb=document.createElement("img");
				var thumbContainer=document.createElement("div");
				var play=document.createElement("div");
				var description=document.createElement("div");
				var title=document.createElement("div");
				var header=document.createElement("div");				
				header.className="plListHeader";
				itemContainer.className="row-fluid";
				item.id="anv-item"+i;
				item.className="plListItem anv-item";			
				item.onclick=onItemSelected;				
				thumb.onload=onImageLoad;
				thumb.style.width="100%";
				thumb.style.height="auto";
				thumb.src=entry.src_image_url;			
				thumbContainer.className="span4 plListThumbCont";
				play.className="plListPlay";
				description.className="anvDescription";
				description.innerHTML=entry.def_description;
				title.className="plListTitle span10";			
				title.innerHTML=entry.def_title;
				thumbContainer.appendChild(header);
				thumbContainer.appendChild(thumb);			
				itemContainer.appendChild(thumbContainer);
				title.appendChild(description);
				itemContainer.appendChild(title);
				item.appendChild(itemContainer);
				header.appendChild(play);
				
				if(i == 0 || (i % 2) == 0) {
					addedRow = false;
					row=document.createElement("div");
					row.className="row-fluid";
					row.appendChild(item);
				} else {
					addedRow = true;
					row.appendChild(item);
					container.appendChild(row);
				}
				console.log("language",entry.language);
			}
		}
		if(addedRow == false) {
			container.appendChild(row);
		}
		function onImageLoad() {
			this.parentNode.style.display="inline-block";
		}	
