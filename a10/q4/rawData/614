function bookmark(enid) { var usid=""; if(usid==""){location.href="/login.php?referrer=/53775/you-can-now-summon-your-tesla-using-the-apple-watch-videoSummon?utm_content=buffer6544e&utm_medium=social&utm_source=twitter.com&utm_campaign=buffer"; }else{ var star = document.getElementById('star_'+enid); var bookmarked = star.getAttribute('data-bookmarked'); if(bookmarked=="0"){ var poststr = "enid=" + enid; makePOSTRequest('/scripts/web-addbookmark.php', poststr, alertContents);} else{ var poststr = "enid=" + enid; makePOSTRequest('/scripts/web-deletebookmark.php', poststr, alertContents);}}}
function bookmarked(results){ var resultsArray = JSON.parse(results); if(resultsArray['success']==1){ document.getElementById('star_'+resultsArray['enid']).src="/images/index/star-icon-on-blue-60x60.svg"; document.getElementById('star_'+resultsArray['enid']).setAttribute('data-bookmarked', "1"); document.getElementById('star_'+resultsArray['enid']).style.display='none'; document.getElementById('star_'+resultsArray['enid']).offsetHeight; document.getElementById('star_'+resultsArray['enid']).style.display='block';}}
function unbookmarked(results){ var resultsArray = JSON.parse(results); if(resultsArray['success']==1){ document.getElementById('star_'+resultsArray['enid']).src="/images/index/star-icon-off-blue-60x60.svg"; document.getElementById('star_'+resultsArray['enid']).setAttribute('data-bookmarked', "0"); document.getElementById('star_'+resultsArray['enid']).style.display='none'; document.getElementById('star_'+resultsArray['enid']).offsetHeight; document.getElementById('star_'+resultsArray['enid']).style.display='block';}}
function grid_share(enid) { if(enid==""){ return; } var sharesheet = document.createElement("div"); sharesheet.setAttribute("class", "grid_share_box"); sharesheet.setAttribute("id", "sharesheet_"+enid); document.getElementById('grid_text_'+enid).appendChild(sharesheet); var sharesheet_title = document.createElement("div"); sharesheet_title.setAttribute("class", "grid_share_title"); sharesheet_title.innerHTML="Share This Article"; sharesheet.appendChild(sharesheet_title); var sharesheet_facebook = document.createElement("img"); sharesheet_facebook.src="/images/index/facebook-icon-64x64.svg"; sharesheet_facebook.setAttribute("class","grid_share_icon"); sharesheet_facebook.setAttribute("onclick", "share_facebook('" + enid + "');"); sharesheet.appendChild(sharesheet_facebook); var sharesheet_twitter = document.createElement("img"); sharesheet_twitter.src="/images/index/twitter-icon-64x64.svg"; sharesheet_twitter.setAttribute("class","grid_share_icon"); sharesheet_twitter.setAttribute("onclick", "share_twitter('" + enid + "');");sharesheet.appendChild(sharesheet_twitter); var sharesheet_googleplus = document.createElement("img"); sharesheet_googleplus.src="/images/index/googleplus-icon-64x64.svg"; sharesheet_googleplus.setAttribute("class","grid_share_icon"); sharesheet_googleplus.setAttribute("onclick", "share_googleplus('" + enid + "');");sharesheet.appendChild(sharesheet_googleplus); var sharesheet_tumblr = document.createElement("img"); sharesheet_tumblr.src="/images/index/tumblr-icon-64x64.svg"; sharesheet_tumblr.setAttribute("class","grid_share_icon"); sharesheet_tumblr.setAttribute("onclick", "share_tumblr('" + enid + "');");sharesheet.appendChild(sharesheet_tumblr); var sharesheet_linkedin = document.createElement("img"); sharesheet_linkedin.src="/images/index/linkedin-icon-64x64.svg"; sharesheet_linkedin.setAttribute("class","grid_share_icon"); sharesheet_linkedin.setAttribute("onclick", "share_linkedin('" + enid + "');");sharesheet.appendChild(sharesheet_linkedin); var sharesheet_pinterest = document.createElement("img"); sharesheet_pinterest.src="/images/index/pinterest-icon-64x64.svg"; sharesheet_pinterest.setAttribute("class","grid_share_icon"); sharesheet_pinterest.setAttribute("onclick", "share_pinterest('" + enid + "');");sharesheet.appendChild(sharesheet_pinterest); var sharesheet_mail = document.createElement("img"); sharesheet_mail.src="/images/index/mail-icon-64x64.svg"; sharesheet_mail.setAttribute("class","grid_share_icon"); sharesheet_mail.setAttribute("onclick", "share_mail('" + enid + "');");sharesheet.appendChild(sharesheet_mail); var sharesheet_close = document.createElement("img"); sharesheet_close.setAttribute("class", "grid_share_close"); sharesheet_close.setAttribute("onclick", "share_close('" + enid + "')"); sharesheet_close.src="/images/index/close-icon-blue-60x60.svg"; sharesheet.appendChild(sharesheet_close); }
function trim(str, chars) { return ltrim(rtrim(str, chars), chars); }
function ltrim(str, chars) { chars = chars || "\\s"; return str.replace(new RegExp("^[" + chars + "]+", "g"), ""); }
function rtrim(str, chars) { chars = chars || "\\s"; return str.replace(new RegExp("[" + chars + "]+$", "g"), ""); }
function makePOSTRequest(url, parameters, resultsFunc) { var http_request = false; if (window.XMLHttpRequest) {  http_request = new XMLHttpRequest(); if (http_request.overrideMimeType) { http_request.overrideMimeType('text/html'); } } else if (window.ActiveXObject) {  try { http_request = new ActiveXObject("Msxml2.XMLHTTP"); } catch (e) { try { http_request = new ActiveXObject("Microsoft.XMLHTTP"); } catch (e) {} } } if (!http_request) { console.log('Cannot create XMLHTTP instance'); return false; } http_request.onreadystatechange= function() { if (http_request.readyState==4) { if (http_request.status==200) { resultsFunc(http_request, url); } } } 
http_request.open('POST', url, true); http_request.setRequestHeader("Content-type", "application/x-www-form-urlencoded"); http_request.send(parameters); }
function makeGETRequest(url, parameters, resultsFunc) { url = url + "?" + parameters; var http_request = false; if (window.XMLHttpRequest) {  http_request = new XMLHttpRequest(); if (http_request.overrideMimeType) { http_request.overrideMimeType('text/html'); } } else if (window.ActiveXObject) {  try { http_request = new ActiveXObject("Msxml2.XMLHTTP"); } catch (e) { try { http_request = new ActiveXObject("Microsoft.XMLHTTP"); } catch (e) {} } } if (!http_request) { console.log('Cannot create XMLHTTP instance'); return false; } http_request.onreadystatechange= function() { if (http_request.readyState==4) { if (http_request.status==200) { resultsFunc(http_request, url); } } } 
http_request.open('GET', url, true); http_request.setRequestHeader("Content-type", "application/x-www-form-urlencoded"); http_request.send(parameters); }

function alertContents(http_request, url) { if (http_request.readyState == 4) { if (http_request.status == 200) { result = trim(http_request.responseText); if(url=="/scripts/livesearch3.php"){ showresults(result); } else if(url=="/scripts/web-addbookmark.php"){ bookmarked(result); } else if(url=="/scripts/web-deletebookmark.php"){ unbookmarked(result); } } else { alert('There was a problem with the request.'); } } }
function livesearch(event) { if (event.which == 13 || event.keyCode == 13) { var terms = document.getElementById("menu_search_field").value; termsArray=terms.split(" "); var termsEncoded = ""; for(i=0; i<termsArray.length; i++) { termsEncoded=termsEncoded + encodeURIComponent(termsArray[i]) + "-";} termsEncoded = termsEncoded.substring(0, termsEncoded.length - 1); location.href="/search/"+termsEncoded; return false; } if(document.getElementById("menu_search_field").value!=''){ /*document.getElementById("searchresults").style.display='block'; */} else{ document.getElementById('thumbnails_scroller').innerHTML = ""; document.getElementById('thumbnails_scroller').innerHTML = window.thumbnailsscroller; return; } var poststr = "q=" + encodeURIComponent( document.getElementById("menu_search_field").value ); makePOSTRequest('/scripts/livesearch3.php', poststr, alertContents); }
function showresults(results){ var resultsArray = JSON.parse(results); if(resultsArray['error']!=""){ } else { document.getElementById('thumbnails_scroller').style.left='0px'; document.getElementById('thumbnails_scroller').innerHTML = ""; var newHTML = ""; for (i = 0; i < resultsArray['results'].length; i++) { newHTML=newHTML + '<div class="thumbnail_box"><div class="thumbnail_image_box"><a href="' + resultsArray['results'][i]['url'] + '" title="' + resultsArray['results'][i]['title'] + '"><img src="' + resultsArray['results'][i]['img128'] + '" srcset="' + resultsArray['results'][i]['img128'] + ' 1x, ' + resultsArray['results'][i]['img256'] + ' 2x" title="' + resultsArray['results'][i]['title'] + '" alt="' + resultsArray['results'][i]['title'] + '"/></a></div><h3 class="thumbnail_h3"><a href="' + resultsArray['results'][i]['url'] + '" title="' + resultsArray['results'][i]['title'] + '"><span class="thumbnail_title">' + resultsArray['results'][i]['title']  + '</span></a></h3></div>'; } newHTML=newHTML + '<div class="thumbnail_box"><div class="thumbnail_title" style="top: 44%;"><a href="/search/results.php?terms=' + encodeURIComponent(document.getElementById("menu_search_field").value) + '">Search More</a></div></div>'; document.getElementById('thumbnails_scroller').innerHTML = newHTML;}}
function adscroll(){ 
	var doc = document.documentElement; 
	var top = (window.pageYOffset || doc.scrollTop)-(doc.clientTop || 0);
	
	for (i = 0; i < window.breakAdDivArray.length; i++) {
		/*console.log("breakAdDiv " + i + " offsetTop: " + document.getElementById(window.breakAdDivArray[i]).getBoundingClientRect().top);*/
		if(document.getElementById(window.breakAdDivArray[i]).getBoundingClientRect().top > -100 && document.getElementById(window.breakAdDivArray[i]).getBoundingClientRect().top < (window.innerHeight + 100) && document.getElementById(window.breakAdDivArray[i]).childNodes.length < 2){  //if ad box is onscreen
			/*console.log("show break ad: " + i);*/	
			googletag.display(window.breakAdDivArray[i]);
			//at this point we are making even the hidden on mobile ones visible. this is okay for now because viewability isn't for mobile yet
		}
	}
	
	for (i = 0; i < window.gridAdDivArray.length; i++) {
		//console.log("addiv " + i + " offsetTop: " + document.getElementById(window.gridAdDivArray[i]).getBoundingClientRect().top);
		if(document.getElementById(window.gridAdDivArray[i]).getBoundingClientRect().top > -100 && document.getElementById(window.gridAdDivArray[i]).getBoundingClientRect().top < (window.innerHeight + 100) && document.getElementById(window.gridAdDivArray[i]).childNodes.length < 2){  //if ad box is onscreen
			/*console.log("show ad: " + i);	*/
			googletag.display(window.gridAdDivArray[i]);
			//googletag.pubads().refresh([window.gridAdSlotArray[i]]);
		
		}
	}
}
function menuscroll(){
	 var doc = document.documentElement; var top = (window.pageYOffset || doc.scrollTop)-(doc.clientTop || 0); if(window.innerWidth<768) {var bar=document.getElementById('mobile_menu'); if(top>0){ bar.style.borderBottom="1px solid #f6f6f6";} else{ bar.style.borderBottom="0px solid #ddd";} }else{var bar = document.getElementById('menu'); var bar_placeholder = document.getElementById('menu_placeholder'); if((Number(top)>Number(bar.offsetTop)) && bar.style.position!='fixed'){  bar.style.position="fixed"; bar.style.borderBottom="1px solid #eee"; document.getElementById('menu_placeholder').style.display='block'; document.getElementsByClassName('menu_logo_box').item(0).style.height='36px'; document.getElementsByClassName('menu_logo_box').item(0).style.top='12px';}else if ((Number(top)<Number(bar_placeholder.offsetTop)) && (bar_placeholder.style.display=='block')){  bar.style.position="relative"; bar.style.borderBottom="0px"; document.getElementById('menu_placeholder').style.display='none';document.getElementsByClassName('menu_logo_box').item(0).style.height='44px';document.getElementsByClassName('menu_logo_box').item(0).style.top='8px';}}};
var services = function () {}
services.gplus = function () {}
services.gplus.cb = function (count) { /*console.log("Google Plus Count: " + count);*/ countGooglePlus(count); }
function getcounts(){
	/*console.log('getcounts()');*/
	var enid = "53775";
	
	//FACEBOOK
	getJSONP( 'http://graph.facebook.com/?id=' + encodeURIComponent("http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video"), 'countFacebook' );
	
	if(enid<24323){
		getJSONP( 'http://graph.facebook.com/?id=' + encodeURIComponent("http://www.iclarified.com/entry/index.php?enid=53775"), 'countFacebook' );
	}
	
	//TWITTER
	getJSONP( 'https://cdn.api.twitter.com/1/urls/count.json?url=' + encodeURIComponent("http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video"), 'countTwitter' );
	
	if(enid<24323){
		getJSONP( 'https://cdn.api.twitter.com/1/urls/count.json?url=' + encodeURIComponent("http://www.iclarified.com/entry/index.php?enid=53775"), 'countTwitter' );
	}
	
	//GOOGLEPLUS
	getJSONP( 'http://share.yandex.ru/gpp.xml?url=' + encodeURIComponent("http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video"));
	
	if(enid<24323){
		getJSONP( 'http://share.yandex.ru/gpp.xml?url=' + encodeURIComponent("http://www.iclarified.com/entry/index.php?enid=53775"));
	}
	
	//PINTEREST
	getJSONP( 'http://api.pinterest.com/v1/urls/count.json?url=' + encodeURIComponent("http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video"), 'countPinterest' );
	
