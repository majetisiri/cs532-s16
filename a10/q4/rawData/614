<style>
#lightview_modal {display: none; position: fixed; top: 0; left: 0; z-index:10000;  width: 100%; height: 100%; padding-top: 30px; padding-bottom: 30px; background: rgba(255, 255, 255, 1);}
#lightview_close { position: absolute; left: 10px; top: 10px; width: 20px; height: 20px; background-image: url(/images/index/close-icon-blue-60x60.svg); background-size: contain; cursor: pointer; z-index:10001;} 
#lightview_frame { position: absolute; width: 100%; height: 100%; background-color: #fff; opacity: 1; z-index:10001; padding-left: 40px; padding-right: 40px; padding-bottom: 60px;}
#lightview_box { position:relative; overflow: scroll !important; -webkit-overflow-scrolling: touch !important; width: 100%; height: 100%;}
#lightview_iframe { position: relative; left: 0px; top: 0px; height: 100%; width: 100%;}
#lightview_left {position: absolute; left: 10px; top: 50%; margin-top: -10px; height: 20px; width: 20px; cursor:pointer;  background-image: url(/images/index/arrow-left-blue-60x60.svg); background-size: contain; z-index:11000;}
#lightview_right {position: absolute; right: 10px; top: 50%; margin-top: -10px; height: 20px; width: 20px; cursor:pointer; background-image: url(/images/index/arrow-right-blue-60x60.svg); background-size: contain; z-index:11000;}
#lightview_index {position: absolute; left: 0px; bottom: 10px; width: 100%; height: 20px; text-align: center;z-index:11000;}
.fadein{ -webkit-animation: fadein 0.2s; -webkit-animation-iteration-count: 1; -webkit-animation-fill-mode: forwards; animation: fadein 0.2s; animation-iteration-count: 1; animation-fill-mode: forwards; }
@-webkit-keyframes fadein { from {opacity: 0} to {opacity: 1} }
@keyframes fadein { from {opacity: 0} to {opacity: 1} }
</style>
<div id="lightview_modal">
    <div id="lightview_close" onClick="lightview_close()"></div>
    <div id="lightview_left" onClick="lightview_prev();"></div>
    <div id="lightview_right" onClick="lightview_next();"></div>
    <div id="lightview_index"></div>
    <div id="lightview_frame">
       	<div id="lightview_box"><iframe id="lightview_iframe" src="" frameborder="0" hspace="0"></iframe></div>     
    </div>
    
</div>

<script>

window.lightview_array=[];
window.lightview_index=-1;

function lightview_open(href){
	var lightview_index_base = "lightview_";
	var lightview_index_number = "";
	var lightview_index_id = "";
	
	for (l=0; l < window.lightview_array.length; l++){
		lightview_index_number = l.toString();
		lightview_index_id = lightview_index_base + lightview_index_number;
		if(href==window.lightview_array[l]){
			window.lightview_index=l;
			if(document.getElementById('lightview_iframe').src!=href){
				document.getElementById('lightview_iframe').src=href;
			}
			document.getElementById(lightview_index_id).style.backgroundColor = '#007aff';
		}
		else{
			document.getElementById(lightview_index_id).style.backgroundColor = '#ddd';	
		}
	}
	
	document.body.style.overflow='hidden';
	document.getElementById('lightview_modal').style.display='block';	
	document.getElementById('lightview_modal').className="fadein";
}

function lightview_close(){
	
	document.getElementById('lightview_modal').style.opacity=0;
	document.getElementById('lightview_modal').style.display="none";
	document.getElementById('lightview_iframe').src = "";
	document.body.style.overflow='auto';
}

function lightview_prev(){
	
	var lightview_index_base = "lightview_";
	var lightview_index_number = "";
	var lightview_index_id = "";
	
	if(window.lightview_index==0){
		return;
	}
	
	window.lightview_index=window.lightview_index-1;
	var href=window.lightview_array[window.lightview_index];
		
	for (l=0; l < window.lightview_array.length; l++){
		lightview_index_number = l.toString();
		lightview_index_id = lightview_index_base + lightview_index_number;
		if(href==window.lightview_array[l]){
			document.getElementById('lightview_iframe').src=href;
			document.getElementById(lightview_index_id).style.backgroundColor = '#007aff';
		}
		else{
			document.getElementById(lightview_index_id).style.backgroundColor = '#ddd';	
		}
	}
}

function lightview_next(){
	var lightview_index_base = "lightview_";
	var lightview_index_number = "";
	var lightview_index_id = "";
	
	if(window.lightview_index==window.lightview_array.length-1){
		return;
	}
	
	window.lightview_index=window.lightview_index+1;
	var href=window.lightview_array[window.lightview_index];
		
	for (l=0; l < window.lightview_array.length; l++){
		lightview_index_number = l.toString();
		lightview_index_id = lightview_index_base + lightview_index_number;
		if(href==window.lightview_array[l]){
			document.getElementById('lightview_iframe').src=href;
			document.getElementById(lightview_index_id).style.backgroundColor = '#007aff';
		}
		else{
			document.getElementById(lightview_index_id).style.backgroundColor = '#ddd';	
		}
	}
}

function lightview_select(selected){
	var lightview_index_base = "lightview_";
	var lightview_index_number = "";
	var lightview_index_id = "";
	
	window.lightview_index=parseInt(selected.getAttribute("data-lightview-index"));
	var href=window.lightview_array[window.lightview_index];
		
	for (l=0; l < window.lightview_array.length; l++){
		lightview_index_number = l.toString();
		lightview_index_id = lightview_index_base + lightview_index_number;
		if(href==window.lightview_array[l]){
			document.getElementById('lightview_iframe').src=href;
			document.getElementById(lightview_index_id).style.backgroundColor = '#007aff';
		}
		else{
			document.getElementById(lightview_index_id).style.backgroundColor = '#ddd';	
		}
	}
}

var anchorElements = document.getElementsByTagName('a');
for (var i in anchorElements){
	if(anchorElements[i].className == "lightview"){
		window.lightview_array.push(anchorElements[i].href);
		anchorElements[i].onclick = function() {
			lightview_open(this.href);
    		return false;
		}
		//create image numbers
		var div = document.createElement("div");
		div.setAttribute("id", "lightview_" +( window.lightview_array.length-1));
		div.setAttribute("data-lightview-index", ( window.lightview_array.length-1));
		div.setAttribute("onclick", "lightview_select(this);");
		div.style.display = "inline-block";
		div.style.margin = "10px 5px 10px 5px";
		//div.style.font = "15px/26px sans-serif";
		//div.style.textAlign = "center";
		div.style.width = "10px";
		div.style.height = "10px";
		div.style.background = "#ddd";
		div.style.borderRadius="5px";
		//div.style.color = "white";
		div.style.cursor = "pointer";
		//div.innerHTML = window.lightview_array.length;
		document.getElementById('lightview_index').appendChild(div);
	}
}
</script>


<script>
window.lastAdWidth = window.innerWidth;
window.socialCountsArray=[];
function gridload(){ if(window.grid!="") { document.getElementById('grid_header').style.display='block'; document.getElementById('grid_container_id').innerHTML=window.grid; document.getElementById('grid_container_id').className = "grid_container"; window.grid=""; var arr = document.getElementById('grid_container_id').getElementsByTagName('script'); for (var n = 0; n < arr.length; n++) { eval(arr[n].innerHTML);} gridlayout();}}
function gridlayout(){ if(window.grid!="") { return; } var boxes = document.getElementsByClassName("grid_box"); var article_images = document.getElementsByTagName("img"); var pattern1260 = [50,25,25,25,50,25,25,25,50,33.33,33.33,33.33,50,50,25,25,25,25]; var pattern1024 = [33.33,33.33,33.33,33.33,33.33,33.33,33.33,33.33,33.33,33.33]; var pattern768 = [50,50,50,50,50,50,50,50,50,50]; var pattern100 = [100,100,100,100,100,100,100,100,100,100]; var j = 0; var retina = window.devicePixelRatio > 1;
	if(window.innerWidth >= 1260){ if(window.lastWidth>=1260){return;}else{window.lastWidth=window.innerWidth;}
		/*console.log("innerwidth>1260");*/
		for (i = 0; i < boxes.length; i++) {
    		boxes[i].style.width = pattern1260[j] + "%"; 
			var imgbox = boxes[i].getElementsByClassName("grid_image_box"); var textbox = boxes[i].getElementsByClassName("grid_text_box"); if(pattern1260[j]>49){ imgbox[0].style.height = '60%'; textbox[0].style.height = '40%'; } if(retina || !retina){imgbox[0].style.backgroundImage=imgbox[0].style.backgroundImage.replace("-640", "-1280"); imgbox[0].style.backgroundImage=imgbox[0].style.backgroundImage.replace("-320", "-640");}
			j=j+1; if(j>=pattern1260.length){j=0;}
		}
	}
	else if(window.innerWidth >= 1024 && window.innerWidth < 1260){ if(window.lastWidth>= 1024 && window.lastWidth < 1260){return;}else{window.lastWidth=window.innerWidth;} /*console.log("innerwidth>1024<1260");*/
		for (i = 0; i < boxes.length; i++) {
    		boxes[i].style.width = pattern1024[j] + "%"; 
			var imgbox = boxes[i].getElementsByClassName("grid_image_box"); var textbox = boxes[i].getElementsByClassName("grid_text_box"); if(pattern1024[j]<50){ imgbox[0].style.height = '50%'; textbox[0].style.height = '50%'; } if(retina){ imgbox[0].style.backgroundImage=imgbox[0].style.backgroundImage.replace("-640", "-1280"); imgbox[0].style.backgroundImage=imgbox[0].style.backgroundImage.replace("-320", "-640");}
			j=j+1; if(j>=pattern1024.length){j=0;}
		}		
	}
	else if(window.innerWidth >= 768 && window.innerWidth < 1024){ if(window.lastWidth>= 768 && window.lastWidth < 1024){return;}else{window.lastWidth=window.innerWidth;} /*console.log("innerwidth>768<1024");*/
		for (i = 0; i < boxes.length; i++) {
    		boxes[i].style.width = pattern768[j] + "%"; 
			var imgbox = boxes[i].getElementsByClassName("grid_image_box"); var textbox = boxes[i].getElementsByClassName("grid_text_box"); if(pattern768[j]<50){ imgbox[0].style.height = '50%'; textbox[0].style.height = '50%'; } if(retina){ imgbox[0].style.backgroundImage=imgbox[0].style.backgroundImage.replace("-640", "-1280"); imgbox[0].style.backgroundImage=imgbox[0].style.backgroundImage.replace("-320", "-640");}
			j=j+1; if(j>=pattern768.length){j=0;}
		}
	}
	else{ if(window.lastWidth < 768){return;}else{window.lastWidth=window.innerWidth;} /*console.log("innerwidth<768");*/ document.getElementById('menu').style.position="relative"; document.getElementById('menu').style.borderBottom="0px"; document.getElementById('menu_placeholder').style.display='none';for(i=0;i<boxes.length;i++){boxes[i].style.width=pattern100[j]+"%";j=j+1;if(j>=pattern100.length){j=0;}}}
}/*end of gridlayout function*/
//layout();
function sidelayout(){
	/*console.log('sidelayout');*/
	window.spanfull=[];
	window.spanfullpositions=[];
	window.spanfullpositions.push(0);
	var offset=0; //the bottom of the last div
	var center=document.getElementById('article_body_column_center');
	var center_images = center.getElementsByTagName('img');
	for(i = 0; i < center_images.length; i++){
		if(center_images[i].getAttribute('data-span') == "full"){
				window.spanfull.push(center_images[i]);
				center_images[i].setAttribute('width', window.innerWidth);
				center_images[i].setAttribute('height', 'auto');
				center_images[i].style.marginLeft='-'+(center.offsetLeft)+'px';
				window.spanfullpositions.push(center_images[i].offsetTop);
				window.spanfullpositions.push(center_images[i].offsetTop+center_images[i].offsetHeight);
		}
		else if(center_images[i].getAttribute('data-span') > 0 && center_images[i].getAttribute('data-span') <=100){
				var multiplier = center_images[i].getAttribute('data-span')/100;
				var newwidth = window.innerWidth*multiplier;
				var newsrc = center_images[i].getAttribute('src');
				newsrc = newsrc.replace("-640", "");
				newsrc = newsrc.replace("-1280", "");
				newsrc = newsrc.replace("-1920", "");
				newsrc = newsrc.replace("-2560", "");
				if(window.devicePixelRatio > 1){
					if(window.innerWidth > 640 && newwidth < 640){ newwidth=640; }
					if(window.innerWidth > 960 && window.innerWidth < 1281){ center_images[i].setAttribute('srcset', newsrc.replace('.', '-1280.') + " 1x, " + newsrc.replace('.', '-1920.') + " 2x"); }
					if(window.innerWidth > 1280){ center_images[i].setAttribute('srcset', newsrc.replace('.', '-1280.') + " 1x, " + newsrc.replace('.', '-2560.') + " 2x"); }
					if(window.innerWidth > 1280){ center_images[i].setAttribute('src', newsrc.replace('.', '-2560.')); }
				}
				else{
					if(window.innerWidth > 640 && newwidth < 640){ newwidth=640; }
					if(window.innerWidth > 1280 && window.innerWidth < 1921){ center_images[i].setAttribute('src', newsrc.replace('.', '-1920.'));}
					if(window.innerWidth > 1280 && window.innerWidth < 1921){ center_images[i].setAttribute('srcset', newsrc.replace('.', '-1920.') + " 1x, " + newsrc.replace('.', '-2560.') + " 2x"); }
					if(window.innerWidth > 1920){ center_images[i].setAttribute('src', newsrc.replace('.', '-2560.')); }	
					if(window.innerWidth > 1920){ center_images[i].setAttribute('srcset', newsrc.replace('.', '-2560.') + " 1x, " + newsrc.replace('.', '.') + " 2x"); }
				}
				window.spanfull.push(center_images[i]);
				center_images[i].setAttribute('width', newwidth);
				center_images[i].setAttribute('height', 'auto');
				center_images[i].style.marginLeft=''+(-1*(((newwidth-center.offsetWidth)/2)))+'px';
				/*console.log(   "(-1*(" + center.offsetLeft+" - ((" + newwidth + "-" + center.offsetWidth + ")/2)))" );*/
				window.spanfullpositions.push(center_images[i].offsetTop);
				window.spanfullpositions.push(center_images[i].offsetTop+center_images[i].offsetHeight);
		}
	}
	var center_iframes = center.getElementsByTagName('iframe');
	for(i = 0; i < center_iframes.length; i++){
		if(center_iframes[i].getAttribute('data-span') > 0 && center_iframes[i].getAttribute('data-span') <=100){
				var multiplier = center_iframes[i].getAttribute('data-span')/100;
				var newwidth = window.innerWidth*multiplier;
				center_iframes[i].setAttribute('width', ''+newwidth+'px');
				center_iframes[i].setAttribute('height', ''+((newwidth*9)/16)+'px');
				center_iframes[i].style.marginLeft=''+(-1*(((newwidth-center.offsetWidth)/2)))+'px';
				window.spanfullpositions.push(center_iframes[i].offsetTop);
				window.spanfullpositions.push(center_iframes[i].offsetTop+center_iframes[i].offsetHeight);
		}
	}
}
function adlayout(){
	if(window.innerWidth >= 1110){ if(window.lastAdWidth>=1110){return;}else{window.lastAdWidth=window.innerWidth;}
		for (i = 0; i < window.gridAdDivArray.length; i++) {
			if(document.getElementById(window.gridAdDivArray[i]).offsetHeight>59){
				/*console.log('refresh slot >1260: ' + i);*/
				googletag.pubads().refresh([window.gridAdSlotArray[i]]);
			}
		}
		googletag.pubads().refresh([window.adLeaderboardSlotArray[0]]);
	}
	else if(window.innerWidth >= 830 && window.innerWidth < 1110){ if(window.lastAdWidth>= 830 && window.lastAdWidth < 1110){return;}else{window.lastAdWidth=window.innerWidth;}
		for (i = 0; i < window.gridAdDivArray.length; i++) {
			if(document.getElementById(window.gridAdDivArray[i]).offsetHeight>59){
				/*console.log('refresh slot 830-1260: ' + i);*/
				googletag.pubads().refresh([window.gridAdSlotArray[i]]);
			}
		}
		googletag.pubads().refresh([window.adLeaderboardSlotArray[0]]);
	}
	else if(window.innerWidth < 830){ if(window.lastAdWidth < 830){return;}else{window.lastAdWidth=window.innerWidth;}
		for (i = 0; i < window.gridAdDivArray.length; i++) {
			if(document.getElementById(window.gridAdDivArray[i]).offsetHeight>59){
				/*console.log('refresh slot <830: ' + i);*/
				googletag.pubads().refresh([window.gridAdSlotArray[i]]);
			}
		}
		googletag.pubads().refresh([window.adLeaderboardSlotArray[0]]);
	}
}
if(window.attachEvent){window.attachEvent('onDOMContentLoaded', function(){ /*after just code loaded  getcounts();*/}); window.attachEvent('onload', function(){ /*after images loaded */ getcounts(); sidelayout(); adlayout(); }); window.attachEvent('onscroll', function(){menuscroll(); gridload(); clearTimeout(window.adscrollTimeout); window.adscrollTimeout = setTimeout(adscroll, 200); }); window.attachEvent('onresize',function(){gridlayout(); clearTimeout(window.sidelayoutTimeout); window.sidelayoutTimeout = setTimeout(sidelayout, 300); clearTimeout(window.adlayoutTimeout); window.adlayoutTimeout = setTimeout(adlayout, 300);/*adlayout();*/}); }else if(window.addEventListener){window.addEventListener('DOMContentLoaded', function(){ /*getcounts();*/}, false); window.addEventListener('load', function(){ getcounts(); sidelayout(); adlayout(); }, false); window.addEventListener('scroll', function(){menuscroll(); gridload(); clearTimeout(window.adscrollTimeout); window.adscrollTimeout = setTimeout(adscroll, 200);}, false); window.addEventListener('resize', function(){gridlayout(); clearTimeout(window.sidelayoutTimeout); window.sidelayoutTimeout = setTimeout(sidelayout, 300); clearTimeout(window.adlayoutTimeout); window.adlayoutTimeout = setTimeout(adlayout, 300);/*adlayout();*/},true);}else{}
function arrow_left(){if((document.getElementById('thumbnails_scroller').offsetLeft+576)>0){document.getElementById('thumbnails_scroller').style.left='0px';}else{var c=document.getElementById('thumbnails_scroller').offsetLeft;c=parseInt(c,10);c=c+576;document.getElementById('thumbnails_scroller').style.left=c+'px';}}
function arrow_right(){ if((document.getElementById('thumbnails_scroller').offsetLeft + document.getElementById('thumbnails_scroller').offsetWidth) < document.getElementById('thumbnails_bar').offsetWidth){ return; } var c = document.getElementById('thumbnails_scroller').offsetLeft; c = parseInt(c, 10); c = c - 576; document.getElementById('thumbnails_scroller').style.left = c + 'px'; }
