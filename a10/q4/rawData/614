	}
	
	//LINKEDIN
	getJSONP( 'https://www.linkedin.com/countserv/count/share?url=' + encodeURIComponent("http://www.iclarified.com/53775/you-can-now-summon-your-tesla-using-the-apple-watch-video"), 'countLinkedIn' );
	
	if(enid<24323){
		getJSONP( 'https://www.linkedin.com/countserv/count/share?url=' + encodeURIComponent("http://www.iclarified.com/entry/index.php?enid=53775"), 'countLinkedIn' );
