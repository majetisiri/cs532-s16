    <!-- google_ad_section_start -->
    <h1 class="cN-headingPage" itemprop="name">Pulled pork and coleslaw burgers with chipotle mayo </h1>
    <!-- google_ad_section_end -->
    <div class="meta hnews">
        <dl>
            <dt class="hiddenVisually">Date</dt>
            <dd class="updated dtstamp">
                <time itemprop="datePublished" datetime="February 3, 2016">
                February 3, 2016</time>
            </dd>
        </dl>
        <!--todo: on UGC-->
        <ul class="tools">
            <li class="reading collectorHidden" style="display:none">(29)</li>
            </ul>
        <!-- Display author details -->
        <div class="author" itemscope itemtype="http://schema.org/Person">
            <div class="cT-headshot">
                    <div>
                            <a href="http://www.dailylife.com.au/dl-food/by/Neil-Perry">
                                    <img alt="Neil Perry" title="Neil Perry" src="/content/dam/images/h/v/e/3/x/image.imgtype.thumbnail.90x90.png/1408400771287.png"></a>
                            </div>
                    <h3 class="authorName">
                            <a href="http://www.dailylife.com.au/dl-food/by/Neil-Perry" rel="author" itemprop="name">Neil Perry</a>
                            </h3>
                    <h4 class="authorTitle" itemprop="jobTitle">Chef and Good Weekend columnist</h4>
                    <p class="authorBlog">
                            <a href="http://www.dailylife.com.au/dl-food/by/Neil-Perry">View
                                more articles from <span class="author-name">Neil Perry</span></a>
                        </p>
                    <p class="authorFollow" itemprop="follows">
                        </p>
                </div>
                <!-- class:cT-headshot -->
            </div>
    <!-- class:author -->

<!-- Display author details
        <div class="zoom hiddenVisually">
            <!-- remove isPublishedAfter from the conditions if one day the keywords of all articles in DCDS are synched with FP --><!-- self here refers to TimelineEntityLinksComponent.java
