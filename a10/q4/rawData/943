		<script>

			jQuery( window ).load( function() {

				var ad = jQuery( '.lxb-rlhb_ad' );

				var paragraphs = jQuery( '.post-content > p' );

				jQuery( paragraphs ).each( function( k, v ) {

					var elementAfterP = jQuery( this ).next();

					var typeOfElementAfterP = jQuery( elementAfterP ).prop( 'tagName' );

					if ( typeOfElementAfterP != 'P' ) {

						jQuery( this ).addClass( 'lxb-rlhb_bad-p' );

					} else if( jQuery( this ).find( 'img' ).length > 0 ) {

						jQuery( this ).addClass( 'lxb-rlhb_bad-p' );

					} else if( jQuery( this ).find( 'script' ).length > 0 ) {

						jQuery( this ).addClass( 'lxb-rlhb_bad-p' );

					} else if( jQuery( elementAfterP ).find( 'img' ).length > 0 ) {

						jQuery( this ).addClass( 'lxb-rlhb_bad-p' );

					}

				});

				var goodParagraphs = jQuery( paragraphs ).not( '.lxb-rlhb_bad-p' );

				var pCount = goodParagraphs.length;

				if( pCount > 6 ) {

					var halfway = Math.floor( pCount / 2 ); 

					var halfP = jQuery( goodParagraphs ).eq( halfway );

					jQuery( ad ).insertAfter( halfP );

				} else {

					jQuery( ad ).appendTo( '.post-content' );

				}
			});

		</script>
	
