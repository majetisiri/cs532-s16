var getNative=__webpack_require__(31),FUNC_ERROR_TEXT="Expected a function",nativeMax=Math.max,nativeNow=getNative(Date,"now"),now=nativeNow||function(){return(new Date).getTime()};module.exports=debounce},/* 31 */
