<script>
    window.adsReady = window.adsReady || {'aol': false};
    var ADS_TIMEOUT = 1000;

    function displayAd(time, id) {
        var timeNow = Date.now();
        //console.log(timeNow - time);
        if (timeNow - time < ADS_TIMEOUT) {
            for(var i in window.adsReady) {
                //console.log(window.adsReady[i]);
                if (window.adsReady[i] === false) {
                    return window.setTimeout(function() {
                        displayAd(time, id);
                    }, 100);
                }
            }
        }

        if( activeAds[id]['lazy_load'] == 'true' ) {
            adInView(id);
        } else {
            try {
                googletag.cmd.push(function () {
                    //console.log('displaying ad');
                    googletag.display(id);
                });
            } catch(e) {
                if( !(e instanceof TypeError) ) {
                    Bugsnag.notifyException(e);
                }
            }
        }
    }
</script>

