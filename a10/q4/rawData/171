<script type="text/javascript">

    if (readCookie('daily') == '') {
        if(readCookie('monthlyCounter') == '' || parseInt(readCookie('monthlyCounter')) <= 30){
            var expiryDate = new Date();
            expiryDate.setDate(expiryDate.getDate() + 1);
            document.cookie = 'daily = 1; expires=' + expiryDate.toGMTString();
            document.cookie = 'isDailyCounted = false; expires=Fri, 31 Dec 9999 23:59:59 GMT';
        }
    }

    if (readCookie('monthly') == '') {
        var expiryDate = new Date();
        expiryDate.setMonth(expiryDate.getMonth() + 1);
        document.cookie = 'monthly = 1; expires=' + expiryDate.toGMTString();
        document.cookie = 'isDailyCounted = true; expires=Fri, 31 Dec 9999 23:59:59 GMT';
        document.cookie = "monthlyCounter=1; expires=Fri, 31 Dec 9999 23:59:59 GMT'" ;
    }
    else if(readCookie('isDailyCounted') === 'false' && parseInt(readCookie('monthlyCounter')) <= 30){
        document.cookie = 'isDailyCounted = true; expires=Fri, 31 Dec 9999 23:59:59 GMT';
        document.cookie = "monthlyCounter=" + (parseInt(readCookie('monthlyCounter')) + 1) + "; expires=Fri, 31 Dec 9999 23:59:59 GMT'" ;
    }

</script>
