<script src="http://www.menshealth.com/sites/menshealth.com/files/js/js_MNPktlIu7q2mongywIRswBbgXPrXL_sqIy5rQZCfHGE.js"></script>
<script>function janrainCaptureWidgetOnLoad() {
  //console.log("zeus_janrain janrainCaptureWidgetOnLoad");
  var loginprovider = '';
  var registrationtype = 'traditional';
  var loginStart = false;
  var loginSuccess = false;
  var loginError = false;
  var loginFound = false;
  var loginToken = false;
  var date = new Date();
  date.setTime(date.getTime() + (30 * 24 * 60 * 60 * 1000));//30 days
  
  //query param parser
  jQuery.extend({
    getQueryParams: function(){
      var vars = [], hash;
      var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
      for(var i = 0; i < hashes.length; i++)
      {
        hash = hashes[i].split('=');
        vars.push(hash[0]);
        vars[hash[0]] = hash[1];
      }
      return vars;
    },
    getQueryParam: function(name){
      return jQuery.getQueryParams()[name];
    }
  });

  function zeus_subscribe_newsletters() {
    //console.log("zeus_janrain zeus_subscribe_newsletters: subscribing to rodale newsletters ");
    
    var ajaxUrl = 'https://secure.rodalequickpay.com/uof/servlet/CoRegServlet';
    //parameters to pass:  email, offerId, optInBrand (hardcode to Y)
    
    //get the rodale default registration event
    var user_mail = jQuery('input#capture_traditionalRegistration_emailAddress').attr('value');
    var nl_form_registration = 'input.nl-form-registration';
    var nl_form_newsletters = 'input.nl-form-input:checked';
    if(typeof(user_mail) == "undefined" || user_mail == "") {
      user_mail = jQuery('input#capture_socialRegistration_emailAddress').attr('value');
      nl_form_registration = 'input.social-nl-form-registration';
      nl_form_newsletters = 'input.social-nl-form-input:checked';
    }
    if(typeof(user_mail) == "undefined" || user_mail == "" ) {
      //console.log("zeus_janrain zeus_subscribe_newsletters: Can not subscribe to Newsletters. No valid email address found.");
      return;
    }
    
    jQuery(nl_form_registration).each(function(){
      var offerId = jQuery(this).attr('value');
      var nlname = jQuery(this).attr('name');
      //Post a ajax request
      jQuery.ajax({
        type: "POST",
        url: ajaxUrl,
        data: {'email' : user_mail, 'offerId' : offerId, 'optInBrand': 'Y'},
        success: function(data, status) {
          if(status) {
            //console.log("zeus_janrain Subscribed to (Default) registration event "+nlname+"  ");
          }
        },
        dataType: 'json'
      });
      //console.log("zeus_janrain default registration event ="+ nlname + " value ="+ offerId);
    });
    
    //get the checked newsletters
    jQuery(nl_form_newsletters).each(function(){
      var offerId = jQuery(this).attr('value');
      var nlname = jQuery(this).attr('name');
       //ajax call
       jQuery.ajax({
        type: "POST",
        url: ajaxUrl,
        data: {'email' : user_mail, 'offerId' : offerId, 'optInBrand': 'Y'},
        success: function(data, status) {
          if(status) {
            //console.log("zeus_janrain Subscribed to (Default) registration event "+nlname+"  ");
          }
        },
        dataType: 'json'
      });
    });
  }
  
  var $logout_link = jQuery('a[href="/user/logout"]');
  $logout_link.addClass('capture_end_session');

  function handleCaptureLogin(result) {
    //console.log("zeus_janrain handleCaptureLogin ");
    jQuery.ajax({
      url: Drupal.settings.basePath + Drupal.settings.pathPrefix + 'janrain_capture/oauth?code=' + result.authorizationCode,
      success: function(token) {
        window.location.reload();
      },
      async: false
    });
  }
  
  /* Adobe analytics 
  event7 - onProviderLoginStart
  event8 - onProviderLoginError
  event9 - onProviderLoginSuccess
  event10 - onProviderLoginComplete
  event11 - onProviderLoginToken
  event14 - onReturnExperienceFound

  eVar6 - Janrain Social Login (provider)
  prop10 - Janrain Social Login (provider) 
  */ 
  
  
  var submitOmnitureEvent = function(event, provider, linktitle){
    if (typeof s_gi === "undefined" || !jQuery.isFunction(s_gi)) {
      //console.log("zeus_janrain Omniture s-code (in particular s_gi function ) is not available.");
      return;      
    } else {
      var s = s_gi(s_account);
      if(event === 'event6') {
        s.linkTrackVars = 'prop10,events,eVar6';
        s.eVar6 = provider;
      } else {
        s.linkTrackVars = 'prop10,events,eVar11';
        s.eVar11 = provider;
      }
      s.linkTrackEvents = event;
      s.events = event;
      s.prop10 = provider;
      s.tl(true, 'o', linktitle);
      //console.log("zeus_janrain Omniture tracking... event="+event+" provider="+provider+" linktitle="+linktitle+" s_account="+s_account);
    }
  };
  
  

  // for the Janrain Enterprise Events API, see:
  // http://developers.janrain.com/documentation/widgets/user-registration-widget/capture-widget-api/events/
  // for adobe analytics info:
  // https://developer.omniture.com/en_US/content_page/sitecatalyst-tagging/c-tagging-overview
  //#1
  janrain.events.onCaptureLoginStart.addHandler(function(result){
    var action = "traditionalAccount";
    if(loginStart) {
      //console.log("zeus_janrain #0 onCaptureLoginStart Login type="+jQuery.cookie("zeusLT")+ "result.action="+result.action);
    }
    if (result.action === "socialSignin" && !loginStart) {
      //action = result.action;
      jQuery.cookie("zeusLT", "socialSignin", { expires: date });
    } else if(result.action !== "socialSignin" && !loginStart) {
      //action = result.action;
      jQuery.cookie("zeusLT", "traditionalSignin", { expires: date, path: '/' });
    } 
    
    if(loginStart) {
    //console.log("zeus_janrain #1 onCaptureLoginStart action="+action+" after cookie set ="+jQuery.cookie("zeusLT"));
    submitOmnitureEvent('event6', result.action, 'onCaptureLoginStart');
    }
    if(!loginStart) {
      loginStart = true;
    } else {
      loginStart = false;
    }
  });
  
  //#2
  janrain.events.onProviderLoginStart.addHandler(function(result){
    var provider = "traditionalAccount";
    jQuery.cookie("zeusLT", "socialSignin", { expires: date, path: '/' });
    if(result !== "undefined" && result.provider !== null && result.provider !== "") {
      provider = result.provider;
      loginprovider = result.provider;
    }
    //console.log("zeus_janrain #2 onProviderLoginStart "+provider);
    submitOmnitureEvent('event7', provider, 'onProviderLoginStart');
  });
  
  //#3
  janrain.events.onProviderLoginSuccess.addHandler(function(){
    //console.log("zeus_janrain #3 onProviderLoginSuccess "+loginprovider);
    //set session cookie
    jQuery.cookie("zeusLS", "socialSignin", {path: '/'});
    submitOmnitureEvent('event9', loginprovider, 'onProviderLoginSuccess');
  });
  
  //#4
  janrain.events.onProviderLoginError.addHandler(function(result){
    var error = '';
    if(result !== "undefined" && result.err !== null) {
      error = result.err[code]  + " " +  result.err[msg] + " " + result.origin + " " + result.status;
    }
    //console.log("zeus_janrain #4 onProviderLoginError "+error);
    submitOmnitureEvent('event8', error, 'onProviderLoginError');
  });
  janrain.events.onCaptureLoginFailed.addHandler(function(){
    //console.log("zeus_janrain #4 onCaptureLoginFailed ");
    submitOmnitureEvent('event8', jQuery.cookie("zeusLT"), 'onCaptureLoginFailed');
  });
  
  janrain.events.onProviderLoginComplete.addHandler(function(){
    //console.log("zeus_janrain #5 onProviderLoginComplete");
    submitOmnitureEvent('event10', loginprovider, 'onProviderLoginComplete');
  });
  
  janrain.events.onProviderLoginToken.addHandler(function(){
    //console.log("zeus_janrain #6 onProviderLoginToken");
    submitOmnitureEvent('event11', loginprovider, 'onProviderLoginToken');
  });
  
  //#5
  janrain.events.onCaptureLoginSuccess.addHandler(handleCaptureLogin);
  janrain.events.onCaptureLoginSuccess.addHandler(function(result){
    var token = "";
    if (result.accessToken != null) {
        token = result.accessToken;
    }
    //console.log("zeus_janrain #7 onCaptureLoginSuccess Login type="+jQuery.cookie("zeusLT"));
    if(jQuery.cookie("zeusLT") === 'traditionalSignin') {
      //set session cookie
      jQuery.cookie("zeusLS", "traditionalSignin", {path: '/'});
      submitOmnitureEvent('event9', jQuery.cookie("zeusLT"), 'onCaptureLoginSuccess');
    }
  });
  
  //#6
  janrain.events.onReturnExperienceFound.addHandler(function(result){
    if(result !== null) {
      //Get session cookie
      var loginsession = jQuery.cookie("zeusLS");
      var loginType = jQuery.cookie("zeusLT");
      //console.log("zeus_janrain #8 onReturnExperienceFound loginsession="+loginsession);
      if(loginType !== "undefined" && loginType !== '' && (loginsession === 'undefined' || loginsession === null)) {
        jQuery.cookie("zeusLS", loginType, {path: '/'});
        submitOmnitureEvent('event14', result.returnProvider, 'onReturnExperienceFound');
      }      
    }
  });
  
  janrain.events.onCaptureSessionNotFound.addHandler(function(result){
    if (typeof(Backplane) != "undefined" && typeof(Backplane.getChannelID()) == "undefined") {
      Backplane.resetCookieChannel();
    }
  });
  
  janrain.events.onCaptureRegistrationStart.addHandler(function(result){
    var action = "";
    if(result.action !== "undefined") {
      action = result.action;
    }
    var screen = "";
    if(result.screen !== "undefined") {
      screen = result.screen;
    }
    //console.log("zeus_janrain #1 onCaptureRegistrationStart action=" +action+" screen="+screen);
    submitOmnitureEvent('event12', screen, 'onCaptureRegistrationStart');
  });

  janrain.events.onCaptureRegistrationSuccess.addHandler(handleCaptureLogin);
  janrain.events.onCaptureRegistrationSuccess.addHandler(function(result){
    //console.log("zeus_janrain #9 onCaptureRegistrationSuccess: User registration was a success. Registering for any newsletters.");
		zeus_subscribe_newsletters();	
		
    //janrain.capture.ui.modal.close();
    if (window.console && window.console.log) console.log(result);

    var provider = "traditionalAccount";
    //set session cookie
    jQuery.cookie("zeusLS", "traditionalSignin", {path: '/'});
    if (result.authProvider != null) {
      provider = result.authProvider;
    }
    
    submitOmnitureEvent('event13', provider, 'onCaptureRegistrationSuccess');
  });
  
  janrain.events.onProviderLogoutStart.addHandler(function(result){
    if(result.provider !== "undefined") {
      //console.log("zeus_janrain #10 onProviderLogoutStart "+result.provider);
    } else {
      //console.log("zeus_janrain #10 onProviderLogoutStart");
    }
  });
  janrain.events.onCaptureRegistrationFailed.addHandler(function(){
    //console.log("zeus_janrain #11 onCaptureRegistrationFailed "+loginprovider);
    submitOmnitureEvent('event9', loginprovider, 'onCaptureRegistrationFailed');
  });
  
  
  

  janrain.events.onCaptureSessionFound.addHandler(function(result){
    var loginsession = jQuery.cookie("zeusLS");
    var loginType = jQuery.cookie("zeusLT");
    var loginUser = jQuery.cookie("zeusHD");
    //console.log("zeus_janrain #12 onCaptureSessionFound loginsession="+loginsession+" loginType="+loginType+ " loginUser="+loginUser);
    if(loginUser !== null && loginType !== null && loginType === 'traditionalSignin' && (loginsession === 'undefined' || loginsession === null)) {
      jQuery.cookie("zeusLS", loginType, {path: '/'});
      submitOmnitureEvent('event14', loginType, 'onReturnExperienceFound');
    }
  });

  janrain.events.onCaptureAccessDenied.addHandler(function() {
    //window.location = '/';
    //console.log("zeus_janrain #13 onCaptureAccessDenied");
    submitOmnitureEvent('event8', 'none', 'onCaptureAccessDenied');
  });

  janrain.events.onCaptureExpiredToken.addHandler(function() {
    //window.location = '/';
    //console.log("zeus_janrain #14 onCaptureExpiredToken");
  });

  janrain.events.onCaptureSessionEnded.addHandler(function() {
    //This is called for both social and traditional logout
    console.log("zeus_janrain #15 onCaptureSessionEnded LoginType="+jQuery.cookie("zeusLT"));
    submitOmnitureEvent('event8', jQuery.cookie("zeusLT"), 'onCaptureSessionEnded');
    jQuery.cookie("zeusLT", '', {expires: 1});
    window.location.href = '/user/logout';
  });

  janrain.events.onCaptureScreenShow.addHandler(function(result) {
    //console.log("zeus_janrain #-1 onCaptureScreenShow");    
  });
  
  //open modal window when user login (ul) param is passed
  var loginvar = jQuery.getQueryParam('ul');
  var dstvar = jQuery.getQueryParam('destination');
  var loginsession = jQuery.cookie("zeusLS");
  var loginUser = jQuery.cookie("zeusHD");
  
  if(typeof loginvar != 'undefined' && loginvar && loginUser == null) {    
    janrain.capture.ui.modal.open(screen);    
  } else if(typeof loginvar != 'undefined' && loginvar && loginUser != "" && typeof dstvar != 'undefined' && dstvar !== "") {
    window.location.replace(window.location.protocol +"//" + window.location.host+"/"+ dstvar);
  }
  
  // should be the last line in janrainCaptureWidgetOnLoad()
  janrain.capture.ui.start();  
}
</script>
