            <div class="article-section bottom-article-section">
          <section class="field-body"><div id="zeus_inhouse_ad_container"><div id="block-dfp-in-content-article-520x150" class="block block-dfp first odd">

      
  <div id="dfp-ad-in_content_article_520x150-wrapper" class="dfp-tag-wrapper in_content_article_520x150-ad">
<div  id="dfp-ad-in_content_article_520x150" class="dfp-tag-wrapper">
    <script type="text/javascript">
        ybotq.push(function() {
          googletag.cmd.push(function() {
            googletag.display("dfp-ad-in_content_article_520x150");
          });
        });
    </script>
