      <script type="text/javascript">
      if(typeof s === 'object') {
      /*
      
       * Plugin: manageVars v1.5 (requires s.pt utility if using AppMeasurement)
      
       */
      
      s.manageVars=new Function("c","l","f",""
      
      +"var s=this,vl,la,vla;l=l?l:'';f=f?f:1;if(!s[c])return false;vl='pag"
      
