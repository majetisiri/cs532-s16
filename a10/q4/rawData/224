                CNBC_Utils.execute('cnbc alerts', function(){
                        var breakingNewsArgs = {
                            mode: 'breaking_news'
                            ,dismiss: true
                        };
                        var proAlertArgs = {
                            mode: 'pro_alert'
                            ,target: true
                            ,hide: true
                            ,animate: true
                            ,dismiss: true
                            ,cookie: true
                        };
                        var watchLiveArgs = {
                            mode: 'watch_live'
                            ,hide: true
                            ,animate: true
                            ,dismiss: true
                            ,cookie: true
                        };
                        var watchLiveIntlArgs = {
                            mode: 'watch_live_intl'
                            ,hide: true
                            ,animate: true
                            ,dismiss: true
                            ,cookie: true
                        };
                        CNBC_Alerts.init(
                            30,
                            //'30',
                            [breakingNewsArgs, proAlertArgs, watchLiveArgs, watchLiveIntlArgs]
                        );
                    });
