	    var symLookup = new CNBC_Symbol_Lookup( { 
			container : '.symbol-search',
			advancedURL: CNBC_Settings.baseurls.advanced_symlookup_URL,
			quoteURL: CNBC_Settings.baseurls.detailedquotebase,
			searchUrl: CNBC_Settings.baseurls.searchbase + '?target=all&categories=exclude&partnerId=2000&keywords='
		} ) ;
		symLookup.init();
	});
	
