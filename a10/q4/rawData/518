<!-- Sponsored Post Tag -->
    
<h1>Chipotle is making 5 major changes to its food</h1>

<div id="content" class="content">
    <div class="post-top">
        <!-- Byline -->
        <div class="flex byline">
			<div class="flex-auto byline-icon">
							<a class="author-photo author-photo-square" href="http://www.businessinsider.com/author/hayley-peterson">
