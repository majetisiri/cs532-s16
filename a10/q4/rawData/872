    (function(d,s){var ia=d.createElement(s);ia.async=1,s=d.getElementsByTagName(s)[0],ia.src=('https:'===d.location.protocol?'https://ssl-':'http://')+'s.idio.co/ia.js',s.parentNode.insertBefore(ia,s)})(document,'script');

    // Custom Idio Tracking
    (function idio(){
        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i=0; i<ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0)==' ') c = c.substring(1);
                if (c.indexOf(name) != -1) return c.substring(name.length,c.length);
            }
            return "";
        }

        var _utma = getCookie("__utma");
        var _ga = getCookie("_ga");
        var uid = null;
        if (_utma != ""){
            uid = _utma.split(".")[1];
        }else if (_ga != ""){
            uid = _ga.split(".")[2];
        }
        if (uid != null){
            _iaq.push(['user','utma', uid]);
            _iaq.push(['track', 'identify']);
        }

        // Is a subscriber
        var sub = getCookie("uref");
        if(sub !== ""){
        	_iaq.push(['user','uref', sub]);
            _iaq.push(['group', 'uref']);
            _iaq.push(['track', 'join']);

        }
    })();
</script>
    
        
