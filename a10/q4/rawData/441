<script>
function calcWallpaper() {
	var containerWidth = $('#main-page-container').width();
	var margin = 30;
	var fullContainer = containerWidth + margin;
	var w = window.innerWidth;
	var x = w - fullContainer; 
	var sides = (x / 2) - 10;
	$("#wallpaper-left").width(sides);
	$("#wallpaper-right").width(sides);
}
        
        
$(document).ready(function() {
	calcWallpaper();
});
        
$(window).bind('resize', function() {
	calcWallpaper();
});  
</script>


