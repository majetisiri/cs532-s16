    if (referrerURL && referrerURL.indexOf('utm_source=s.tucson.com&utm_medium=social&utm_campaign=share') > -1)  found = true;
    if (referrerURL && referrerURL.indexOf('utm_source=tucne.ws&utm_medium=social&utm_campaign=share') > -1)  found = true;
