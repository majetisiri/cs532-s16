<script type="text/javascript">
var gSP_local_pass = true;
var gSP_data = false;
function gSP_local_callback(data) {
    gSP_data = data;
    if (gSP_data.meta.status != 0 || !gSP_data.response.hasMarketAccess)  gSP_local_pass = false;
}

jQuery(function() {
    if (gSP_local_pass && typeof(gSP_data) == 'object')
    {
        jQuery('.gannett-tn-user-screenname').html(jQuery('<div/>').text(gSP_data.response.fullName));
        jQuery('.gannett-logged-in').show();
    }
    else
    {
        jQuery('.gannett-not-logged-in').show();
    }
});
</script>
<script type="text/javascript" src="https://account.tucson.com/default/gateway/getUser/?callback=gSP_local_callback&_ts=1461817701"></script>

