function ad_loaded(ad_type) {

    var ad_body = document.getElementById(ad_type).children[0] ;

    if( (ad_body  !== undefined) && (ad_body.height > 0 ) ){
      return true ;
     } else {
      return false ;
    }

 }

function ad_refresh(ad_type) {

        var ad_body = document.getElementById(ad_type).children[0] ;
        var iframe_load =  setInterval(function(){
              if(ad_loaded(ad_type)) {
                  console.log( ad_type + ' will reload in 20sec') ;
                  var AdChanger = setTimeout(function(){ refreshAd(ad_type); }, 20000);
                  clearInterval(iframe_load);
                    }

                  }, 1000);

             }
              function refreshAd(ad_type) {
                        document.getElementById(ad_type).children[0].contentWindow.location.reload();
                        console.log(ad_type + ' has loaded ') ;
                }
